source_set("cookie_util") {
  configs += [ "//build/config/compiler:enable_arc" ]
  sources = [
    "cookie_util.h",
    "cookie_util.mm",
  ]
  deps = [
    "//base",
    "//ios/net",
    "//ios/web/common",
    "//ios/web/public",
    "//net",
    "//net:extras",
  ]
}

source_set("unit_tests") {
  configs += [ "//build/config/compiler:enable_arc" ]
  testonly = true
  sources = [ "cookie_util_unittest.mm" ]
  deps = [
    ":cookie_util",
    "//base",
    "//base/test:test_support",
    "//ios/net",
    "//ios/net:test_support",
    "//ios/web/common",
    "//ios/web/public/test",
    "//testing/gtest",
  ]
}
