[package]
name = "unicode-linebreak"
version = "0.1.2"
authors = ["Axel Forsman <axelsfor@gmail.com>"]
description = "Implementation of the Unicode Line Breaking Algorithm"
homepage = "https://github.com/axelf4/unicode-linebreak"
