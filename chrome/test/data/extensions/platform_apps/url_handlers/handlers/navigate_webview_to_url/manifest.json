{
  "name": "Handler: navigate webview",
  "manifest_version": 2,
  "version": "1",
  "app": {
    "background": {
      "scripts": ["test.js"]
    }
  },

  "permissions": [
    "webview"
  ],

  "url_handlers": {
    "my_doc_url": {
      "title": "Open my doc",
      "matches": [
        "http://localhost:*/extensions/platform_apps/url_handlers/*"
      ]
    }
  }
}
