// https://wicg.github.io/shape-detection-api/#enumdef-landmarktype
enum LandmarkType { "mouth", "eye", "nose" };

// https://wicg.github.io/shape-detection-api/#dictdef-landmark
dictionary Landmark {
  required FrozenArray<Point2D> locations;
  LandmarkType type;
};
