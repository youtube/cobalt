source_set("unit_tests") {
  testonly = true
  configs += [ "//build/config/compiler:enable_arc" ]
  sources = [ "command_dispatcher_unittest.mm" ]
  deps = [
    ":commands",
    "//base",
    "//testing/gtest",
  ]
}
