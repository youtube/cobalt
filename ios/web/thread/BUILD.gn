import("//ios/build/config.gni")

source_set("thread") {
  deps = [
    "//build:blink_buildflags",
    "//ios/web:threads",
    "//ios/web/public",
  ]

  if (use_blink) {
    deps += [ "//ios/web/content:threads" ]
  }

  sources = [ "web_thread.cc" ]
}
