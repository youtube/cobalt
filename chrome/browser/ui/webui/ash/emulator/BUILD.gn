  deps = [
    "//ash/webui/common:trusted_types_util",
    "//chrome/browser:resources",
    "//chrome/browser/profiles:profile",
    "//chrome/browser/ui/webui/ash/bluetooth",
    "//chromeos/ash/components/dbus",
    "//chromeos/ash/components/dbus/audio",
    "//chromeos/dbus/power",
  ]
