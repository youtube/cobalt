{
  "name": "Test",
  "version": "0.0.1",
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["redirect.html"],
      "matches": ["<all_urls>"],
      "use_dynamic_url": true
    }
  ],
  "background": { "service_worker": "service_worker.js" },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
