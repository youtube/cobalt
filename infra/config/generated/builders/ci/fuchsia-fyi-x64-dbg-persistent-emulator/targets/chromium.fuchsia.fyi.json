{
  "fuchsia-fyi-x64-dbg-persistent-emulator": {
    "additional_compile_targets": [
      "all"
    ],
    "gtest_tests": [
      {
        "args": [
          "--everlasting"
        ],
        "merge": {
          "script": "//testing/merge_scripts/standard_gtest_merge.py"
        },
        "name": "aura_unittests",
        "resultdb": {
          "enable": true,
          "inv_extended_properties_dir": "${ISOLATED_OUTDIR}/invocations"
        },
        "swarming": {
          "dimensions": {
            "kvm": "1",
            "os": "Ubuntu-20.04",
            "pool": "chromium.tests.fuchsia"
          },
          "named_caches": [
            {
              "name": "fuchsia_emulator_cache",
              "path": ".fuchsia_emulator/fuchsia-everlasting-emulator"
            }
          ],
          "service_account": "chromium-tester@chops-service-accounts.iam.gserviceaccount.com"
        },
        "test": "aura_unittests",
        "test_id_prefix": "ninja://ui/aura:aura_unittests/"
      },
