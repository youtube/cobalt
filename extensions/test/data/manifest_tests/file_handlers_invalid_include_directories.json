{
  "name": "test",
  "manifest_version": 2,
  "description": "App with file_handlers manifest.",
  "version": "1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "file_handlers": {
    "text": {
      "types": [
        "text/*"
      ]
    },
    "directories": {
      "extensions": ["*"],
      "include_directories": "invalid_value"
    }
  }
}
