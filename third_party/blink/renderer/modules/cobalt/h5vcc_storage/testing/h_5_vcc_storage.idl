dictionary H5vccStorageWriteTestResponse {
  long? bytes_written;
  DOMString? error;
};

dictionary H5vccStorageVerifyTestResponse {
  long? bytes_read;
  DOMString? error;
  boolean verified;
};

// This interface mirrors the H5vccSystem Web IDL defined in Cobalt LTS 25:
// https://github.com/youtube/cobalt/blob/25.lts.stable/cobalt/h5vcc/h5vcc_system.idl
[
  ImplementedAs=H5vccStorageForTesting,
  Exposed=Window,
  SecureContext
] partial interface H5vccStorage {
  H5vccStorageWriteTestResponse writeTest(unsigned long test_size, DOMString test_string);
  H5vccStorageVerifyTestResponse verifyTest(unsigned long test_size, DOMString test_string);
};
