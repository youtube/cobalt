import("//build/config/chromeos/ui_mode.gni")
import("//mojo/public/tools/bindings/mojom.gni")

assert(is_chromeos_ash, "Demo Mode App is ash-chrome only")

mojom("mojom") {
  sources = [ "demo_mode_app_untrusted_ui.mojom" ]
  public_deps = [ "//mojo/public/mojom/base" ]
  webui_module_path = "/ash/webui/demo_mode_app_ui/mojom/"

  # This page still uses JS + Closure compiler.
  generate_webui_js_bindings = true
}
