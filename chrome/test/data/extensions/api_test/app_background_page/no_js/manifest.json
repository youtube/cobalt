{
  "name": "app_background_page/no_js",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Tests that window.open returns null without JS access to background page.",
  "background": {
    "scripts": ["test.js"]
  },
  "permissions": ["tabs", "http://a.com/*"],
  "content_scripts": [
    {
      "matches": ["http://a.com/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}
