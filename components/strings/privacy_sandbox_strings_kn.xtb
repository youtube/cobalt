<!DOCTYPE translationbundle>
<translationbundle lang="kn">
<translation id="1444443021901415601">ನಿಮ್ಮ ಇತ್ತೀಚಿನ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸದ ಆಧಾರದ ಮೇಲೆ ಆಸಕ್ತಿಯ ವಿಷಯಗಳನ್ನು Chrome ಗುರುತು ಹಾಕಿಕೊಳ್ಳುತ್ತದೆ</translation>
<translation id="2252052615723281030">ನೀವು Chrome ಗೆ ಸೈನ್ ಇನ್ ಮಾಡಿದಾಗ, ಈ ಸೆಟ್ಟಿಂಗ್ ನಿಮ್ಮ IP ವಿಳಾಸವನ್ನು ಖಾಸಗಿಯಾಗಿರಿಸಲು ಸಹಾಯ ಮಾಡುತ್ತದೆ. ಸೈಟ್ ನಿಮ್ಮನ್ನು ಟ್ರ್ಯಾಕ್ ಮಾಡುತ್ತಿರಬಹುದು ಎಂದು Chrome ಅನುಮಾನಿಸಿದರೆ, ನಿಮ್ಮ ಕೆಲವು ಟ್ರಾಫಿಕ್ ಅನ್ನು ಗೌಪ್ಯತೆ ಸರ್ವರ್‌ಗಳ ಮೂಲಕ ಕಳುಹಿಸಬಹುದು.</translation>
<translation id="235789365079050412">Google ಗೌಪ್ಯತೆ ನೀತಿ</translation>
<translation id="2793385791865768072">ನಿಮ್ಮ ಇತ್ತೀಚಿನ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸದ ಆಧಾರದ ಮೇಲೆ ಆಸಕ್ತಿಯ ವಿಷಯಗಳನ್ನು Chromium ಗುರುತು ಹಾಕಿಕೊಳ್ಳುತ್ತದೆ.</translation>
<translation id="2979365474350987274">ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳು ಸೀಮಿತವಾಗಿವೆ</translation>
<translation id="33479675708419823">ನೀವು Chrome ಗೆ ಸೈನ್ ಇನ್ ಮಾಡಿದಾಗ, ಈ ಸೆಟ್ಟಿಂಗ್ ನಿಮ್ಮ IP ವಿಳಾಸವನ್ನು ಖಾಸಗಿಯಾಗಿರಿಸಲು ಸಹಾಯ ಮಾಡುತ್ತದೆ. ಸೈಟ್ ನಿಮ್ಮನ್ನು ಟ್ರ್ಯಾಕ್ ಮಾಡುತ್ತಿರಬಹುದು ಎಂದು Chrome ಅನುಮಾನಿಸಿದರೆ, ನಿಮ್ಮ ಕೆಲವು ಟ್ರಾಫಿಕ್ ಅನ್ನು ಗೌಪ್ಯತೆ ಸರ್ವರ್‌ಗಳ ಮೂಲಕ ಕಳುಹಿಸಬಹುದು. <ph name="LINK_BEGIN" />IP ಪ್ರೊಟೆಕ್ಷನ್ ಹೇಗೆ ಕಾರ್ಯನಿರ್ವಹಿಸುತ್ತದೆ<ph name="LINK_END" /> ಎಂಬುದರ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ</translation>
<translation id="3529311220178827364">ನಿಮ್ಮ IP ವಿಳಾಸವನ್ನು ಮರೆಮಾಡುವ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ</translation>
<translation id="385051799172605136">ಹಿಂದಕ್ಕೆ</translation>
<translation id="4041687306270459969">4 ವಾರಗಳಿಗಿಂತ ಹಳೆಯದಾದ ಆ್ಯಡ್ ವಿಷಯಗಳನ್ನು ಸ್ವಯಂಚಾಲಿತವಾಗಿ ಅಳಿಸಲಾಗುತ್ತದೆ. ಕ್ರೀಡೆಗಳು, ಉಡುಪುಗಳು ಹಾಗೂ ಮುಂತಾದವುಗಳ ಕುರಿತು ವಿಷಯಗಳು ಒಳಗೊಂಡಿರುತ್ತವೆ</translation>
<translation id="4177501066905053472">ಆ್ಯಡ್ ವಿಷಯಗಳು</translation>
<translation id="4301151630239508244">ಆ್ಯಡ್ ವಿಷಯಗಳು ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತೀಕರಿಸಲು ಸೈಟ್ ಬಳಸಬಹುದಾದ ಹಲವು ವಿಷಯಗಳಲ್ಲಿ ಒಂದಾಗಿದೆ. ಆ್ಯಡ್ ವಿಷಯಗಳು ಇಲ್ಲದಿದ್ದರೂ ಸಹ, ಸೈಟ್‌ಗಳು ನಿಮಗೆ ಆ್ಯಡ್‌ಗಳನ್ನು ತೋರಿಸಬಹುದು, ಆದರೆ ಅವುಗಳು ಕಡಿಮೆ ವೈಯಕ್ತೀಕರಿಸುತ್ತದೆ. <ph name="BEGIN_LINK_1" />ನಿಮ್ಮ ಆ್ಯಡ್‌ ಗೌಪ್ಯತೆಯನ್ನು ನಿರ್ವಹಿಸುವ<ph name="END_LINK_1" /> ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="4501357987281382712">ನಮ್ಮ <ph name="BEGIN_LINK" />ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ<ph name="END_LINK" /> ನಿಮ್ಮ ಡೇಟಾವನ್ನು Google ಹೇಗೆ ರಕ್ಷಿಸುತ್ತದೆ ಎಂಬುದರ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="453692855554576066">ನಿಮ್ಮ ಆ್ಯಡ್ ವಿಷಯಗಳನ್ನು ನೀವು Chromium ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ನೋಡಬಹುದು ಮತ್ತು ಸೈಟ್‌ಗಳಲ್ಲಿ ಹಂಚಿಕೊಳ್ಳಲು ಬಯಸದ ವಿಷಯಗಳನ್ನು ನಿರ್ಬಂಧಿಸಬಹುದು</translation>
<translation id="4598427486995631491">ನಿಮ್ಮ IP ವಿಳಾಸವನ್ನು ಮರೆಮಾಡಿ</translation>
<translation id="4692439979815346597">ನಿಮ್ಮ ಆ್ಯಡ್ ವಿಷಯಗಳನ್ನು ನೀವು Chrome ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ನೋಡಬಹುದು ಮತ್ತು ಸೈಟ್‌ಗಳಲ್ಲಿ ಹಂಚಿಕೊಳ್ಳಲು ಬಯಸದ ವಿಷಯಗಳನ್ನು ನಿರ್ಬಂಧಿಸಬಹುದು</translation>
<translation id="5413586988657235240">ನೀವು ಭೇಟಿ ನೀಡುವ ಸೈಟ್‌ಗಳು ನಿಮ್ಮ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸ ಮತ್ತು ಗುರುತನ್ನು ಸುರಕ್ಷಿತವಾಗಿರಿಸುವಾಗ ನಿಮಗೆ ಸಂಬಂಧಿಸಿದ ಜಾಹೀರಾತುಗಳನ್ನು ತೋರಿಸುವ ವಿಷಯಗಳಿಗಾಗಿ Chromium ಅನ್ನು ಕೇಳಬಹುದು</translation>
<translation id="6053735090575989697">ನಮ್ಮ ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ ನಿಮ್ಮ ಡೇಟಾವನ್ನು Google ಹೇಗೆ ರಕ್ಷಿಸುತ್ತದೆ ಎಂಬುದರ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
