    "test_mock.ts",
    "test_plural_string_proxy.ts",
    "test_util.ts",
    "test_open_window_proxy.ts",
  ]

  if (!is_android) {
    in_files += [
      "chrome_timeticks_test.ts",
      "fake_settings_private.ts",
      "metrics_test_support.ts",
      "mocked_metrics_reporter.ts",
      "mouse_mock_interactions.ts",
