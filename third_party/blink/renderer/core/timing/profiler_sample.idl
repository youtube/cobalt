// https://wicg.github.io/js-self-profiling/#the-profilersample-dictionary
// https://github.com/WICG/js-self-profiling/pull/64
enum ProfilerMarker { "gc", "layout", "paint", "script", "style" };

dictionary ProfilerSample {
  required DOMHighResTimeStamp timestamp;
  unsigned long long stackId;
  [RuntimeEnabled=ExperimentalJSProfilerMarkers, CrossOriginIsolated] ProfilerMarker marker;
};
