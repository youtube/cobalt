{
  "name": "Content Script API Tests",
  "version": "1",
  "manifest_version": 2,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://*/*/functions.html"],
      "js": ["functions.js"]
    },
    {
      "matches": ["*://*/*/events.html"],
      "js": ["events.js"]
    }
  ]
}
