{
  "name": "incognito no script",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Checks that content scripts do not inject js into incognito browsers.",
  "permissions": ["http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["change_page_title.js"],
      "run_at": "document_end"
    }
  ]
}
