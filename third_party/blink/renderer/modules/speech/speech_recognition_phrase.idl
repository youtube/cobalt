[
    Exposed=Window,
    RuntimeEnabled=WebSpeechRecognitionContext,
    SecureContext
]
interface SpeechRecognitionPhrase {
    [
        CallWith=ScriptState,
        RaisesException
    ]
    constructor(DOMString phrase, optional float boost = 1.0);
    attribute DOMString phrase;
    attribute float boost;
};
