#ifndef TOOLS_CLANG_PLUGINS_TESTS_SYSTEM_MAP_
#define TOOLS_CLANG_PLUGINS_TESTS_SYSTEM_MAP_

namespace std {

template<typename Key, typename Value>
struct map {
  map();
  map(const map&);
  map(map&&);
  ~map();
  map& operator=(const map&);
  map& operator=(map&&);
  unsigned int size() { return 0; }
  void insert() {}
  void erase() {}
};

}  // namespace std

#endif
