// Represents all possible reading formats described in
// https://w3c.github.io/sensors/#algorithms-for-parsing-readings
dictionary VirtualSensorReading {
  // https://w3c.github.io/sensors/#parse-xyz-reading
  double x;
  double y;
  double z;

  // https://w3c.github.io/sensors/#parse-single-value-number-reading
  // Specifically for Ambient Light Sensors.
  double illuminance;

  // https://w3c.github.io/sensors/#parse-quaternion-reading
  FrozenArray<double> quaternion;
};
