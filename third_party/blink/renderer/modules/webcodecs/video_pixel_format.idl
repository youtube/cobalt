// https://github.com/WICG/web-codecs

enum VideoPixelFormat {
    // 4:2:0 Y, U, V
    "I420",
    "I420P10",
    "I420P12",

    // 4:2:0 Y, U, V, A
    "I420A",
    "I420AP10",

    // 4:2:2 Y, U, V
    "I422",
    "I422P10",
    "I422P12",

    // 4:2:2 Y, U, V, A
    "I422A",
    "I422AP10",

    // 4:4:4 Y, U, V
    "I444",
    "I444P10",
    "I444P12",

    // 4:4:4 Y, U, V, A
    "I444A",
    "I444AP10",

    // 4:2:0 Y, UV
    "NV12",

    // 4:4:4 RGBA
    "RGBA",

    // 4:4:4 RGBX (opaque)
    "RGBX",

    // 4:4:4 BGRA
    "BGRA",

    // 4:4:4 BGRX (opaque)
    "BGRX",
};
