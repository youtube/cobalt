module mojo_base.mojom;

// Corresponds to `base::ByteSize` in base/byte_size.h.
struct ByteSize {
  uint64 size;
};

// Corresponds to `base::ByteSizeDelta` in base/byte_size.h.
struct ByteSizeDelta {
  int64 delta;
};
