				val ThreadingModel = s 'Apartment'
			}
			Version = s '%VERSION%'
		}
    ForceRemove %CP_FILTER_CLASS_GUID% = s 'Google Credential Provider Filter Class'
		{
			InprocServer32 = s '%MODULE%'
