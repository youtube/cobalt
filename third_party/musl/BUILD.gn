config("system_includes") {
  libmusl_prefix = "//third_party/musl"
  musl_flags = [
    # Disable the use of standard include directories to ensure system include
    # paths such as /usr/include and /usr/local/include are not used.
    # Note: inspect the paths used with:
    # $ path_to/clang -v -x c++ - -fsyntax-only -nostdlibinc < /dev/null
    "-nostdlibinc",
    # Add starboard/include first, to allow it to take precedence over other
    # files in musl
    "-isystem" + rebase_path("$libmusl_prefix/src/starboard/include",
                              root_build_dir),
    # This is the folder with the directly externally useable include files.
    "-isystem" + rebase_path("$libmusl_prefix/include", root_build_dir),
    # Add the arch folder before the generic to allow generic to be the fallback
    # when there is no arch specific include file.
    "-isystem" +
        rebase_path("$libmusl_prefix/arch/$musl_arch", root_build_dir),
    "-isystem" +
        rebase_path("$libmusl_prefix/arch/generic", root_build_dir),
    # Compile with TLS emulation.
    "-femulated-tls",
  ]
  # These have to be in cflags_c and cflags_cc, to ensure that they are
  # in the search path after libc++.
  cflags_c = musl_flags
  cflags_cc = musl_flags

  # This section ensures that all files depending on this configuration are
  # recompiled whenever a musl header is modified. By adding these headers
  # as 'inputs' to this config, GN correctly tracks the dependency. This is
  # particularly effective for hermetic builds where this config is applied
  # globally via //build/config/c++/BUILD.gn.
  # Using 'inputs' here is more robust than localized 'public_deps' because it
  # provides project-wide coverage without the risk of creating dependency
  # cycles.
  inputs = []
  foreach(header, musl_all_headers) {
    inputs += [ "$libmusl_prefix/$header" ]
  }
}

group("c") {
  public_configs = [ ":system_includes" ]
  deps = [ ":c_internal" ]
}

