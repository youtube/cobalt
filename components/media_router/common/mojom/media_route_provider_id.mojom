module media_router.mojom;

// Each MediaRouteProvider is associated with a unique ID.
// This enum is in its own file to avoid circular dependencies between
// typemapped C++ classes and Mojo structs such as MediaSink.
enum MediaRouteProviderId {
  WIRED_DISPLAY,
  CAST,
  DIAL,
  ANDROID_CAF,
  TEST,
};
