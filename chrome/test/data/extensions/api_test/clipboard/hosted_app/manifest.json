{
  "name": "chrome.clipboard.hosted_app",
  "version": "0.1",
  "manifest_version": 2,
  "description": "end-to-end browser test for clipboard permissions in hosted apps",
  "app": {
    "urls": [
      "http://localhost/extensions/api_test/clipboard/hosted_app"
    ],
    "launch": {
      "web_url": "http://localhost/extensions/api_test/clipboard/hosted_app/main.html"
    }
  },
  "permissions": ["clipboardRead", "clipboardWrite"]
}
