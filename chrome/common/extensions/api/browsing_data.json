      },
      {
        "name": "removePluginData",
        "description": "Clears plugins' data.",
        "deprecated": "Support for Flash has been removed. This function has no effect.",
        "type": "function",
        "parameters": [
          {
            "$ref": "RemovalOptions",
            "name": "options"
