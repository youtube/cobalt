// https://www.khronos.org/registry/webgl/extensions/EXT_clip_control/

[
    LegacyNoInterfaceObject
] interface EXTClipControl {
    const unsigned long LOWER_LEFT_EXT = 0x8CA1;
    const unsigned long UPPER_LEFT_EXT = 0x8CA2;

    const unsigned long NEGATIVE_ONE_TO_ONE_EXT = 0x935E;
    const unsigned long ZERO_TO_ONE_EXT         = 0x935F;

    const unsigned long CLIP_ORIGIN_EXT     = 0x935C;
    const unsigned long CLIP_DEPTH_MODE_EXT = 0x935D;

    void clipControlEXT(GLenum origin, GLenum depth);
};
