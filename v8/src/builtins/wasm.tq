    case (hn: HeapNumber): deferred {
      let value = Convert<float64>(hn);
      if (Float64IsNaN(value)) goto Failure;
      value = math::Float64Trunc(value);
      return ChangeFloat64ToIntPtr(value);
    }
    case (JSAny): deferred {
      goto Failure;
    }
  }
}

