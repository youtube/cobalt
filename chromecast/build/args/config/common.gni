# Args common to all Chromecast browser builds.
is_clang = true
is_component_build = false
enable_cast_receiver = true

# Used with streaming application.
cast_streaming_enable_remoting = true
proprietary_codecs = true

ffmpeg_branding = "Chrome"

# Disable PGO profile as it requires additional gclient configuration.
chrome_pgo_phase = 0

enable_arcore = false
enable_cardboard = false
enable_openxr = false
enable_vr = false
use_chromecast_cdms = false
