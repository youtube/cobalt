    "ArrowDown",
    "ArrowLeft",
    "ArrowRight",
    "ArrowUp",
    "End",
    "Home",
    "PageDown",
    "PageUp",
    {name: "Enter", Symbol: "kCapitalEnter"},
    "Escape",
    "Tab",

    // common media types
    "text/html",
    "application/xml",
