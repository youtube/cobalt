import("//build/config/chromeos/ui_mode.gni")
import("//mojo/public/tools/bindings/mojom.gni")

assert(is_chromeos_ash)

mojom("mojom") {
  disable_variants = true

  sources = [ "boca.mojom" ]

  webui_module_path = "/"

  public_deps = [
