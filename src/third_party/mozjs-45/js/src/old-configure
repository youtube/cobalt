    cat >> confdefs.pytmp <<\EOF
    (''' JS_GC_SMALL_CHUNK_SIZE ''', ' 1 ')
EOF
cat >> confdefs.h <<\EOF
#define JS_GC_SMALL_CHUNK_SIZE 1
EOF

fi

