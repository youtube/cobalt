if test "$JS_STANDALONE"; then
  
if test "$no_recursion" != yes; then
  trap '' EXIT
  if ! $PYTHON $_topsrcdir/build/subconfigure.py --list subconfigures --skip skip_subconfigures; then
      exit 1
  fi
fi


fi

rm -fr confdefs* $ac_clean_files
