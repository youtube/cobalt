    "step-up",
    "step-down",
    // invokeaction for fullscreen
    "toggle-fullscreen",
    "request-fullscreen",
    "exit-fullscreen",
    // invokeaction for audio and video
    "play-pause",
    "pause",
    "play",
    "toggle-muted",

    // loading attribute
    // https://github.com/scott-little/lazyload#ways-the-loading-attribute-can-be-used
    "lazy",
    "eager",
    "auto",

