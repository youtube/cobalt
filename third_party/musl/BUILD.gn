if (target_cpu == "x64") {
  musl_arch = "x86_64"
} else if (target_cpu == "x86") {
  musl_arch = "i386"
} else if (target_cpu == "arm") {
  musl_arch = "arm"
} else if (target_cpu == "arm64") {
  musl_arch = "aarch64"
}

config("external_settings") {
  include_dirs = [
    "arch/$musl_arch",
    "arch/generic",
  ]
}

group("c") {
  public_configs = [ ":external_settings" ]
  deps = [ ":c_internal" ]
}

