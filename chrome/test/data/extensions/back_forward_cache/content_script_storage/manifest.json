{
  "name": "no caching",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Checks that content scripts receiving events evict from back forward cache.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://a.com/*"],
      "js": ["storage_listener.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://b.com/*"],
      "js": ["storage_changer.js"],
      "run_at": "document_end"
    }
  ]
}
