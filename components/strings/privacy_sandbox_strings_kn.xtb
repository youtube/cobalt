<translation id="390681677935721732">ನೀವು ಸೈಟ್‌ಗಳೊಂದಿಗೆ ಹಂಚಿಕೊಂಡಿರುವ ಯಾವುದೇ ಚಟುವಟಿಕೆಯ ಡೇಟಾವನ್ನು Chrome 30 ದಿನಗಳ ನಂತರ ಅಳಿಸುತ್ತದೆ. ನೀವು ಮತ್ತೊಮ್ಮೆ ಸೈಟ್‌ಗೆ ಭೇಟಿ ನೀಡಿದರೆ, ಅದು ಪಟ್ಟಿಯಲ್ಲಿ ಪುನಃ ಕಾಣಿಸಿಕೊಳ್ಳಬಹುದು. <ph name="BEGIN_LINK" />Chrome ನಲ್ಲಿ ನಿಮ್ಮ ಆ್ಯಡ್‌ ಗೌಪ್ಯತೆಯನ್ನು ನಿರ್ವಹಿಸುವ<ph name="END_LINK" /> ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="3918378745482005425">ಕೆಲವು ಫೀಚರ್‌ಗಳು ಕಾರ್ಯನಿರ್ವಹಿಸದಿರಬಹುದು. ಸಂಬಂಧಿತ ಸೈಟ್‌ಗಳು ಇನ್ನೂ ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳನ್ನು ಬಳಸಬಹುದು.</translation>
<translation id="3918927280411834522">ಸೈಟ್ ಸೂಚಿಸಿದ ಆ್ಯಡ್‌ಗಳು.</translation>
<translation id="4009365983562022788">ಸೈಟ್‌ಗಳಾದ್ಯಂತ ನಿಮ್ಮನ್ನು ಟ್ರ್ಯಾಕ್ ಮಾಡಲು ಕಂಪನಿಗಳು ಈ ಡೇಟಾವನ್ನು ಬಳಸುವುದಿಲ್ಲ ಎಂದು ಸಾರ್ವಜನಿಕವಾಗಿ ಅವುಗಳು ಘೋಷಿಸಬೇಕೆಂದು Google ಬಯಸುತ್ತದೆ. ಕೆಲವು ಸೈಟ್‌ಗಳು ಆ್ಯಡ್‌ಗಳನ್ನು ಮಾತ್ರವಲ್ಲದೆ ನಿಮ್ಮ ಅನುಭವವನ್ನೂ ವೈಯಕ್ತಿಕಗೊಳಿಸಲು ನಿಮ್ಮ ಚಟುವಟಿಕೆಯನ್ನು ಬಳಸಬಹುದು. ಅವುಗಳು ನಿಮ್ಮ ಕುರಿತು ಈಗಾಗಲೇ ಹೊಂದಿರುವ ಇತರ ಮಾಹಿತಿಯೊಂದಿಗೆ ಅದನ್ನು ಸಂಯೋಜಿಸಬಹುದು. ನಿಮ್ಮ ಡೇಟಾವನ್ನು ಕಂಪನಿಗಳು ಹೇಗೆ ಬಳಸುತ್ತವೆ ಎಂಬುದರ ಕುರಿತು ನಿಮಗೆ ತಿಳಿಸಲು ಅವುಗಳು ಜವಾಬ್ದಾರರಾಗಿರುತ್ತವೆ. ನಮ್ಮ <ph name="BEGIN_LINK1" />ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ<ph name="LINK_END1" /> ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="4177501066905053472">ಆ್ಯಡ್ ವಿಷಯಗಳು</translation>
<translation id="4278390842282768270">ಅನುಮತಿಸಲಾಗಿದೆ</translation>
<translation id="4301151630239508244">ಆ್ಯಡ್ ವಿಷಯಗಳು ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತೀಕರಿಸಲು ಸೈಟ್ ಬಳಸಬಹುದಾದ ಹಲವು ವಿಷಯಗಳಲ್ಲಿ ಒಂದಾಗಿದೆ. ಆ್ಯಡ್ ವಿಷಯಗಳು ಇಲ್ಲದಿದ್ದರೂ ಸಹ, ಸೈಟ್‌ಗಳು ನಿಮಗೆ ಆ್ಯಡ್‌ಗಳನ್ನು ತೋರಿಸಬಹುದು, ಆದರೆ ಅವುಗಳು ಕಡಿಮೆ ವೈಯಕ್ತೀಕರಿಸುತ್ತದೆ. <ph name="BEGIN_LINK_1" />ನಿಮ್ಮ ಆ್ಯಡ್‌ ಗೌಪ್ಯತೆಯನ್ನು ನಿರ್ವಹಿಸುವ<ph name="END_LINK_1" /> ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="4370439921477851706">ಸೈಟ್‌ಗಳಾದ್ಯಂತ ನಿಮ್ಮನ್ನು ಟ್ರ್ಯಾಕ್ ಮಾಡಲು ಕಂಪನಿಗಳು ಈ ಡೇಟಾವನ್ನು ಬಳಸುವುದಿಲ್ಲ ಎಂದು ಸಾರ್ವಜನಿಕವಾಗಿ ಅವುಗಳು ಘೋಷಿಸಬೇಕೆಂದು Google ಬಯಸುತ್ತದೆ. ಕೆಲವು ಸೈಟ್‌ಗಳು ಆ್ಯಡ್‌ಗಳನ್ನು ಮಾತ್ರವಲ್ಲದೆ ನಿಮ್ಮ ಅನುಭವವನ್ನೂ ವೈಯಕ್ತೀಕರಿಸಲು ನಿಮ್ಮ ಚಟುವಟಿಕೆಯನ್ನು ಬಳಸಬಹುದು. ಅವುಗಳು ಆ್ಯಡ್ ವಿಷಯಗಳನ್ನು 4 ವಾರಗಳಿಗಿಂತ ಹೆಚ್ಚು ಕಾಲ ಸಂಗ್ರಹಿಸಬಹುದು ಮತ್ತು ನಿಮ್ಮ ಬಗ್ಗೆ ಅವುಗಳಿಗೆ ಈಗಾಗಲೇ ತಿಳಿದಿರುವ ಇತರ ಮಾಹಿತಿಯೊಂದಿಗೆ ಅದನ್ನು ಸಂಯೋಜಿಸಬಹುದು. ನಿಮ್ಮ ಡೇಟಾವನ್ನು ಅವರು ಹೇಗೆ ಬಳಸುತ್ತಾರೆ ಎಂಬುದರ ಕುರಿತು ನಿಮಗೆ ತಿಳಿಸಲು ಕಂಪನಿಗಳು ಜವಾಬ್ದಾರರಾಗಿರುತ್ತವೆ. ನಮ್ಮ <ph name="BEGIN_LINK1" />ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ<ph name="LINK_END1" /> ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="4412992751769744546">ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳನ್ನು ಅನುಮತಿಸಿ</translation>
<translation id="4456330419644848501">ನೀವು ನೋಡುವ ಆ್ಯಡ್‌ ವೈಯಕ್ತಿಕಗೊಳಿಸಲಾಗಿದೆಯೇ ಎಂಬುದು ಈ ಸೆಟ್ಟಿಂಗ್, <ph name="BEGIN_LINK_1" />ಸೈಟ್-ಸೂಚಿಸಿದ ಆ್ಯಡ್‌ಗಳು<ph name="END_LINK_1" />, ನಿಮ್ಮ <ph name="BEGIN_LINK_2" />ಕುಕಿ ಸೆಟ್ಟಿಂಗ್‌ಗಳು<ph name="END_LINK_2" /> ಮತ್ತು ನೀವು ವೀಕ್ಷಿಸುತ್ತಿರುವ ಸೈಟ್ ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸುತ್ತದೆಯೇ ಎಂಬುದನ್ನು ಒಳಗೊಂಡಂತೆ ಹಲವಾರು ವಿಷಯಗಳನ್ನು ಆಧರಿಸಿರುತ್ತದೆ.</translation>
<translation id="4501357987281382712">ನಮ್ಮ <ph name="BEGIN_LINK" />ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ<ph name="END_LINK" /> ನಿಮ್ಮ ಡೇಟಾವನ್ನು Google ಹೇಗೆ ರಕ್ಷಿಸುತ್ತದೆ ಎಂಬುದರ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="4502954140581098658">ನೀವು ನೋಡುವ ಆ್ಯಡ್‌ ಅನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸಲಾಗಿದೆಯೇ ಎಂಬುದು ಈ ಸೆಟ್ಟಿಂಗ್, <ph name="BEGIN_LINK_1" />ಆ್ಯಡ್ ವಿಷಯಗಳು<ph name="END_LINK_1" />, ನಿಮ್ಮ <ph name="BEGIN_LINK_2" />ಕುಕಿ ಸೆಟ್ಟಿಂಗ್‌ಗಳು<ph name="END_LINK_2" /> ಮತ್ತು ನೀವು ವೀಕ್ಷಿಸುತ್ತಿರುವ ಸೈಟ್ ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸುತ್ತದೆಯೇ ಎಂಬುದನ್ನು ಒಳಗೊಂಡಂತೆ ಹಲವಾರು ವಿಷಯಗಳನ್ನು ಅವಲಂಬಿಸಿರುತ್ತದೆ.</translation>
<translation id="453692855554576066">ನಿಮ್ಮ ಆ್ಯಡ್ ವಿಷಯಗಳನ್ನು ನೀವು Chromium ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ನೋಡಬಹುದು ಮತ್ತು ಸೈಟ್‌ಗಳಲ್ಲಿ ಹಂಚಿಕೊಳ್ಳಲು ಬಯಸದ ವಿಷಯಗಳನ್ನು ನಿರ್ಬಂಧಿಸಬಹುದು</translation>
<translation id="4616029578858572059">ನಿಮ್ಮ ಇತ್ತೀಚಿನ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸದ ಆಧಾರದ ಮೇಲೆ ಆಸಕ್ತಿಯ ವಿಷಯಗಳನ್ನು Chromium ಗುರುತು ಹಾಕಿಕೊಳ್ಳುತ್ತದೆ. ಉದಾಹರಣೆಗೆ, ಕ್ರೀಡೆ, ಉಡುಪು ಹಾಗೂ ಇನ್ನೂ ಮುಂತಾದವುಗಳ ಕುರಿತು ವಿಷಯಗಳು</translation>
<translation id="4687718960473379118">ಸೈಟ್ ಸೂಚಿಸಿದ ಆ್ಯಡ್‌ಗಳು</translation>
<translation id="4692439979815346597">ನಿಮ್ಮ ಆ್ಯಡ್ ವಿಷಯಗಳನ್ನು ನೀವು Chrome ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ನೋಡಬಹುದು ಮತ್ತು ಸೈಟ್‌ಗಳಲ್ಲಿ ಹಂಚಿಕೊಳ್ಳಲು ಬಯಸದ ವಿಷಯಗಳನ್ನು ನಿರ್ಬಂಧಿಸಬಹುದು</translation>
<translation id="4711259472133554310">ನಿರ್ದಿಷ್ಟ ಸೈಟ್‌ಗಳು ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳನ್ನು ಬಳಸಲು ಯಾವಾಗಲೂ ಅನುಮತಿಸಲು ನೀವು ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ವಿನಾಯಿತಿಗಳನ್ನು ರಚಿಸಬಹುದು</translation>
<translation id="4894490899128180322">ಒಂದು ಸೈಟ್ ನಿರೀಕ್ಷೆಯಂತೆ ಕಾರ್ಯನಿರ್ವಹಿಸದಿದ್ದರೆ, ನೀವು ಭೇಟಿ ನೀಡುತ್ತಿರುವ ನಿರ್ದಿಷ್ಟ ಸೈಟ್‌ಗಾಗಿ ತಾತ್ಕಾಲಿಕವಾಗಿ ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳನ್ನು ಅನುಮತಿಸಲು ಪ್ರಯತ್ನಿಸಬಹುದು</translation>
<translation id="4995684599009077956">ಸೈಟ್‌ಗಳಾದ್ಯಂತ ನಿಮ್ಮನ್ನು ಟ್ರ್ಯಾಕ್ ಮಾಡಲು ಕಂಪನಿಗಳು ಈ ಡೇಟಾವನ್ನು ಬಳಸುವುದಿಲ್ಲ ಎಂದು ಸಾರ್ವಜನಿಕವಾಗಿ ಅವುಗಳು ಘೋಷಿಸಬೇಕೆಂದು Google ಬಯಸುತ್ತದೆ. ಕೆಲವು ಸೈಟ್‌ಗಳು ಆ್ಯಡ್‌ಗಳನ್ನು ಮಾತ್ರವಲ್ಲದೆ ನಿಮ್ಮ ಅನುಭವವನ್ನೂ ವೈಯಕ್ತೀಕರಿಸಲು ನಿಮ್ಮ ಚಟುವಟಿಕೆಯನ್ನು ಬಳಸಬಹುದು. ಅವುಗಳು ಆ್ಯಡ್ ವಿಷಯಗಳನ್ನು 4 ವಾರಗಳಿಗಿಂತ ಹೆಚ್ಚು ಕಾಲ ಸಂಗ್ರಹಿಸಬಹುದು ಮತ್ತು ನಿಮ್ಮ ಬಗ್ಗೆ ಅವುಗಳಿಗೆ ಈಗಾಗಲೇ ತಿಳಿದಿರುವ ಇತರ ಮಾಹಿತಿಯೊಂದಿಗೆ ಅದನ್ನು ಸಂಯೋಜಿಸಬಹುದು. ನಿಮ್ಮ ಡೇಟಾವನ್ನು ಅವರು ಹೇಗೆ ಬಳಸುತ್ತಾರೆ ಎಂಬುದರ ಕುರಿತು ನಿಮಗೆ ತಿಳಿಸಲು ಕಂಪನಿಗಳು ಜವಾಬ್ದಾರರಾಗಿರುತ್ತವೆ. <ph name="BEGIN_LINK" />ನಮ್ಮ ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ<ph name="END_LINK" />.</translation>
<translation id="5055880590417889642">ಆ್ಯಡ್‌ಗಳನ್ನು ಸೂಚಿಸಲು ಸೈಟ್ ಬಳಸಬಹುದಾದ ಹಲವಾರು ವಿಷಯಗಳಲ್ಲಿ ನಿಮ್ಮ ಚಟುವಟಿಕೆಯೂ ಒಂದಾಗಿದೆ. ಸೈಟ್-ಸೂಚಿಸಿದ ಆ್ಯಡ್‌ಗಳನ್ನು ಆಫ್ ಮಾಡಿದಾಗಲೂ, ಸೈಟ್‌ಗಳು ನಿಮಗೆ ಈಗಲೂ ಆ್ಯಡ್‌ಗಳನ್ನು ತೋರಿಸಬಹುದು ಆದರೆ ಅವುಗಳನ್ನು ಕಡಿಮೆ ವೈಯಕ್ತಿಕಗೊಳಿಸಿರಬಹುದು. ಇದರ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ</translation>
<translation id="5117284457376555514">ಸಂಬಂಧಿತ ಸೈಟ್‌ಗಳು ಅವುಗಳನ್ನು ಆ್ಯಕ್ಸೆಸ್ ಮಾಡಲು ನೀವು ಅನುಮತಿಸದ ಹೊರತು, ಕಂಟೆಂಟ್‌ ಮತ್ತು ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸಲು ಮತ್ತು ಇತರ ಸೈಟ್‌ಗಳಲ್ಲಿ ನಿಮ್ಮ ಕ್ರಿಯೆಗಳನ್ನು ಅರ್ಥಮಾಡಿಕೊಳ್ಳಲು ಸೈಟ್‌ಗಳು ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳನ್ನು ಬಳಸುವಂತಿಲ್ಲ. ಕೆಲವು ಸೈಟ್ ಫೀಚರ್‌ಗಳು ನಿರೀಕ್ಷೆಯಂತೆ ಕಾರ್ಯನಿರ್ವಹಿಸದೇ ಇರಬಹುದು.</translation>
<translation id="5165490319523240316">ನೀವು ಭೇಟಿ ನೀಡುವ ಸೈಟ್‌ಗಳಲ್ಲಿ ನೀವು ಹೇಗೆ ನಿಮ್ಮ ಸಮಯವನ್ನು ಕಳೆಯುತ್ತೀರಿ ಎಂಬಂತಹ ನಿಮ್ಮ ಚಟುವಟಿಕೆಯನ್ನು ಸೈಟ್‌ಗಳು ಮತ್ತು ಅವುಗಳ ಜಾಹೀರಾತು ಪಾಲುದಾರರು ಇತರ ಸೈಟ್‌ಗಳಲ್ಲಿ ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸಲು ಬಳಸಬಹುದು. ಉದಾಹರಣೆಗೆ, ನೀವು ರಾತ್ರಿಯ ಊಟಕ್ಕೆ ರೆಸಿಪಿಗಳನ್ನು ಹುಡುಕಲು ಸೈಟ್‌ಗೆ ಭೇಟಿ ನೀಡಿದರೆ, ನೀವು ಅಡುಗೆಯಲ್ಲಿ ಆಸಕ್ತಿ ಹೊಂದಿರಬಹುದು ಎಂದು ಸೈಟ್ ನಿರ್ಧರಿಸಬಹುದು. ನಂತರ, ಮೊದಲ ಸೈಟ್ ಸೂಚಿಸಿದ ದಿನಸಿ ಡೆಲಿವರಿ ಸೇವೆಗೆ ಸಂಬಂಧಿಸಿದ ಆ್ಯಡ್‌ ಅನ್ನು ಮತ್ತೊಂದು ಸೈಟ್ ನಿಮಗೆ ತೋರಿಸಬಹುದು.</translation>
<translation id="544199055391706031">ಆ್ಯಡ್‌ಗಳನ್ನು ಸೂಚಿಸಲು ಸೈಟ್ ಬಳಸಬಹುದಾದ ಹಲವಾರು ವಿಷಯಗಳಲ್ಲಿ ನಿಮ್ಮ ಚಟುವಟಿಕೆಯೂ ಒಂದಾಗಿದೆ. ಸೈಟ್-ಸೂಚಿಸಿದ ಆ್ಯಡ್‌ಗಳನ್ನು ಆಫ್ ಮಾಡಿದಾಗಲೂ, ಸೈಟ್‌ಗಳು ನಿಮಗೆ ಈಗಲೂ ಆ್ಯಡ್‌ಗಳನ್ನು ತೋರಿಸಬಹುದು ಆದರೆ ಅವುಗಳನ್ನು ಕಡಿಮೆ ವೈಯಕ್ತಿಕಗೊಳಿಸಿರಬಹುದು. <ph name="BEGIN_LINK" />ಸೈಟ್ ಸೂಚಿಸಿದ ಆ್ಯಡ್‌ಗಳ<ph name="END_LINK" /> ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="5574580428711706114">ಸೈಟ್‌ಗಳಾದ್ಯಂತ ನಿಮ್ಮನ್ನು ಟ್ರ್ಯಾಕ್ ಮಾಡಲು ಕಂಪನಿಗಳು ಈ ಡೇಟಾವನ್ನು ಬಳಸುವುದಿಲ್ಲ ಎಂದು ಸಾರ್ವಜನಿಕವಾಗಿ ಅವುಗಳು ಘೋಷಿಸಬೇಕೆಂದು Google ಬಯಸುತ್ತದೆ. ನಿಮ್ಮ ಡೇಟಾವನ್ನು ಅವರು ಹೇಗೆ ಬಳಸುತ್ತಾರೆ ಎಂಬುದರ ಕುರಿತು ನಿಮಗೆ ತಿಳಿಸಲು ಕಂಪನಿಗಳು ಜವಾಬ್ದಾರರಾಗಿರುತ್ತವೆ. <ph name="BEGIN_LINK1" />ನಮ್ಮ ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ<ph name="LINK_END1" />.</translation>
<translation id="5677928146339483299">ನಿರ್ಬಂಧಿಸಲಾಗಿದೆ</translation>
<translation id="5759648952769618186">ವಿಷಯಗಳು ನಿಮ್ಮ ಇತ್ತೀಚಿನ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸವನ್ನು ಆಧರಿಸಿವೆ ಮತ್ತು ನಿಮಗೆ ವೈಯಕ್ತೀಕರಿಸಿದ ಜಾಹೀರಾತುಗಳನ್ನು ತೋರಿಸಲು ಸೈಟ್‌ಗಳು ಮತ್ತು ಅವುಗಳ ಜಾಹೀರಾತು ಪಾಲುದಾರರು ನಿಮ್ಮ ಬಗ್ಗೆ ಯಾವ ಮಾಹಿತಿಯನ್ನು ಪಡೆಯಬಹುದು ಎಂಬುದನ್ನು ಮಿತಿಗೊಳಿಸಲು ಸಹಾಯ ಮಾಡುತ್ತದೆ</translation>
<translation id="5812448946879247580"><ph name="BEGIN_BOLD" />ಸೈಟ್‌ಗಳು ಈ ಡೇಟಾವನ್ನು ಹೇಗೆ ಬಳಸುತ್ತವೆ?<ph name="END_BOLD" /> ನೀವು ಭೇಟಿ ನೀಡುವ ಸೈಟ್‌ಗಳು ತಮ್ಮ ಆ್ಯಡ್‌ಗಳ ಪರ್ಫಾರ್ಮೆನ್ಸ್ ಅನ್ನು ಮಾಪನ ಮಾಡುವುದಕ್ಕಾಗಿ ಅವುಗಳಿಗೆ ಸಹಾಯ ಮಾಡುವ ಮಾಹಿತಿಗಾಗಿ Chrome ಅನ್ನು ಕೇಳಬಹುದು. ಇನ್‌ಫರ್ಮೆಶನಲ್ ಸೈಟ್‌ಗಳು ಪರಸ್ಪರ ಹಂಚಿಕೊಳ್ಳಬಹುದಾದ ಮಾಹಿತಿಯನ್ನು ಮಿತಿಗೊಳಿಸುವ ಮೂಲಕ Chrome ನಿಮ್ಮ ಗೌಪ್ಯತೆಯನ್ನು ರಕ್ಷಿಸುತ್ತದೆ.</translation>
<translation id="6053735090575989697">ನಮ್ಮ ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ ನಿಮ್ಮ ಡೇಟಾವನ್ನು Google ಹೇಗೆ ರಕ್ಷಿಸುತ್ತದೆ ಎಂಬುದರ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="6195163219142236913">ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳು ಸೀಮಿತವಾಗಿವೆ</translation>
<translation id="6196640612572343990">ಥರ್ಡ್ ಪಾರ್ಟಿ ಕುಕೀಗಳನ್ನು ನಿರ್ಬಂಧಿಸಿ</translation>
<translation id="6282129116202535093">ನಿಮಗೆ ಸೂಕ್ತವಾದ ಆ್ಯಡ್‌ಗಳನ್ನು ತೋರಿಸಲು ಸೈಟ್‌ಗಳನ್ನು ಅನುಮತಿಸುವಾಗ ನಿಮ್ಮ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸ ಮತ್ತು ಗುರುತನ್ನು ರಕ್ಷಿಸಲು ಸೈಟ್ ಸೂಚಿಸಿದ ಆ್ಯಡ್‌ಗಳು ಸಹಾಯ ಮಾಡುತ್ತವೆ. ನಿಮ್ಮ ಚಟುವಟಿಕೆಯನ್ನು ಬಳಸಿಕೊಂಡು, ನೀವು ಬ್ರೌಸ್ ಮಾಡುವುದನ್ನು ಮುಂದುವರಿಸಿದಾಗ ಇತರ ಸೈಟ್‌ಗಳು ಸಂಬಂಧಿತ ಆ್ಯಡ್‌ಗಳನ್ನು ಸೂಚಿಸಬಹುದು. ನೀವು ಈ ಸೈಟ್‌ಗಳ ಪಟ್ಟಿಯನ್ನು ನೋಡಬಹುದು ಮತ್ತು ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ನಿಮಗೆ ಬೇಡವಾದವುಗಳನ್ನು ನಿರ್ಬಂಧಿಸಬಹುದು.</translation>
<translation id="6308169245546905162">ನೀವು ಇತರ ಸೈಟ್‌ಗಳಲ್ಲಿ ತೆಗೆದುಕೊಳ್ಳುವ ಕ್ರಮಗಳನ್ನು ಅರ್ಥಮಾಡಿಕೊಳ್ಳಲು ಸೈಟ್‌ಗಳು ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳನ್ನು ಬಳಸಬಹುದು</translation>
<translation id="6398358690696005758">ನಮ್ಮ <ph name="BEGIN_LINK1" />ಗೌಪ್ಯತೆ ನೀತಿ<ph name="LINK_END1" /> ಯಲ್ಲಿ ನಿಮ್ಮ ಡೇಟಾವನ್ನು Google ಹೇಗೆ ರಕ್ಷಿಸುತ್ತದೆ ಎಂಬುದರ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ.</translation>
<translation id="6494210367693221285">ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕಿಗಳನ್ನು ಆ್ಯಕ್ಸೆಸ್ ಮಾಡಲು ವೆಬ್‌ಸೈಟ್‌ಗಳಿಗೆ ಅನುವು ಮಾಡಿಕೊಡಲು ನಿಮ್ಮ ಪ್ರಸ್ತುತ Chromium ಸೆಟ್ಟಿಂಗ್‌ಗಳನ್ನು ಅಪ್‌ಡೇಟ್ ಮಾಡಲಾಗಿದೆ. ನೀವು ಯಾವಾಗ ಬೇಕಾದರೂ ಈ ಸೆಟ್ಟಿಂಗ್ ಅನ್ನು ಬದಲಾಯಿಸಬಹುದು.</translation>
<translation id="6702015235374976491">ನಿಮ್ಮ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸ ಮತ್ತು ಗುರುತನ್ನು ರಕ್ಷಿಸುವ ಸಂದರ್ಭದಲ್ಲಿ ನಿಮಗೆ ಸೂಕ್ತವಾದ ಆ್ಯಡ್‌ಗಳನ್ನು ತೋರಿಸಲು ಆ್ಯಡ್‌ ವಿಷಯಗಳು ಸೈಟ್‌ಗಳಿಗೆ ಸಹಾಯ ಮಾಡುತ್ತವೆ. ನಿಮ್ಮ ಇತ್ತೀಚಿನ ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸದ ಆಧಾರದ ಮೇಲೆ ಆಸಕ್ತಿದಾಯಕ ವಿಷಯಗಳನ್ನು Chrome ಗಮನಿಸುತ್ತದೆ. ನಂತರ, ನೀವು ಭೇಟಿ ನೀಡುವ ಸೈಟ್, ನೀವು ನೋಡುವ ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸಲು ಸೂಕ್ತವಾದ ವಿಷಯಗಳಿಗಾಗಿ Chrome ಅನ್ನು ಕೇಳಬಹುದು.</translation>
<translation id="6710025070089118043">ಸಂಬಂಧಿತ ಸೈಟ್‌ಗಳು ಅವುಗಳನ್ನು ಆ್ಯಕ್ಸೆಸ್ ಮಾಡಲು ನೀವು ಅನುಮತಿ ನೀಡದ ಹೊರತು, ಕಂಟೆಂಟ್‌ ಮತ್ತು ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸಲು ಮತ್ತು ಇತರ ಸೈಟ್‌ಗಳಲ್ಲಿ ನಿಮ್ಮ ಕ್ರಿಯೆಗಳನ್ನು ಅರ್ಥಮಾಡಿಕೊಳ್ಳಲು ಸೈಟ್‌ಗಳು ಥರ್ಡ್-ಪಾರ್ಟಿ ಕುಕೀಗಳನ್ನು ಬಳಸುವಂತಿಲ್ಲ</translation>
<translation id="6774168155917940386">ಸೈಟ್‌ಗಳಾದ್ಯಂತ ನಿಮ್ಮನ್ನು ಟ್ರ್ಯಾಕ್ ಮಾಡಲು ಕಂಪನಿಗಳು ಈ ಡೇಟಾವನ್ನು ಬಳಸುವುದಿಲ್ಲ ಎಂದು ಸಾರ್ವಜನಿಕವಾಗಿ ಅವುಗಳು ಘೋಷಿಸಬೇಕೆಂದು Google ಬಯಸುತ್ತದೆ. ಕೆಲವು ಸೈಟ್‌ಗಳು ಆ್ಯಡ್‌ಗಳನ್ನು ಮಾತ್ರವಲ್ಲದೆ ನಿಮ್ಮ ಅನುಭವವನ್ನೂ ವೈಯಕ್ತಿಕಗೊಳಿಸಲು ನಿಮ್ಮ ಚಟುವಟಿಕೆಯನ್ನು ಬಳಸಬಹುದು. ಅವುಗಳು ನಿಮ್ಮ ಕುರಿತು ಈಗಾಗಲೇ ಹೊಂದಿರುವ ಇತರ ಮಾಹಿತಿಯೊಂದಿಗೆ ಅದನ್ನು ಸಂಯೋಜಿಸಬಹುದು. ನಿಮ್ಮ ಡೇಟಾವನ್ನು ಕಂಪನಿಗಳು ಹೇಗೆ ಬಳಸುತ್ತವೆ ಎಂಬುದರ ಕುರಿತು ನಿಮಗೆ ತಿಳಿಸಲು ಅವುಗಳು ಜವಾಬ್ದಾರರಾಗಿರುತ್ತವೆ. <ph name="BEGIN_LINK" />ನಮ್ಮ ಗೌಪ್ಯತೆ ನೀತಿಯಲ್ಲಿ ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ<ph name="END_LINK" />.</translation>
<translation id="6789193059040353742">ನೀವು ನೋಡುವ ಆ್ಯಡ್‌ ವೈಯಕ್ತಿಕಗೊಳಿಸಲಾಗಿದೆಯೇ ಎಂಬುದು ಈ ಸೆಟ್ಟಿಂಗ್, <ph name="BEGIN_LINK1" />ಆ್ಯಡ್ ವಿಷಯಗಳು<ph name="LINK_END1" />, ನಿಮ್ಮ <ph name="BEGIN_LINK2" />ಕುಕಿ ಸೆಟ್ಟಿಂಗ್‌ಗಳು<ph name="LINK_END2" /> ಮತ್ತು ನೀವು ವೀಕ್ಷಿಸುತ್ತಿರುವ ಸೈಟ್ ಆ್ಯಡ್‌ಗಳನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸುತ್ತದೆಯೇ ಎಂಬುದನ್ನು ಒಳಗೊಂಡಂತೆ ಹಲವಾರು ವಿಷಯಗಳನ್ನು ಅವಲಂಬಿಸಿರುತ್ತದೆ.</translation>
