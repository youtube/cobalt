{
  "name": "A page action for testing chrome.pageAction getters",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs", "http://*/*"
  ],
  "page_action": {
    "default_title": "Title",
    "default_popup": "Popup.html"
  }
}
