  "name": "Content Verification ./ paths Test",
  "version": "0.9",
  "manifest_version": 2,
  "background": {
    "scripts": ["./background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.chromium.org/*"],
      "js": ["./cs1.js", "/cs2.js"]
