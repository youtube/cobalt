// https://w3c.github.io/web-locks/#enumdef-lockmode
enum LockMode { "shared", "exclusive" };

// https://w3c.github.io/web-locks/#dictdef-lockoptions
dictionary LockOptions {
    LockMode mode = "exclusive";
    boolean ifAvailable = false;
    boolean steal = false;
    AbortSignal signal;
};
