[JavaPackage="org.chromium.webshare.mojom"]
module blink.mojom;

enum ShareError {
  // Share successful.
  OK,
  // Something went wrong in the implementation.
  INTERNAL_ERROR,
  // Potentially dangerous files.
  PERMISSION_DENIED,
  // Share was canceled by user.
  CANCELED,
};
