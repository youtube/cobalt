# Configuration for an Android-12 (S, API 31) AVD on google_atd on x86_64

emulator_package {
  package_name: "chromium/third_party/android_sdk/public/emulator"
  version: "N2X90_ADQtGKgoxkXZH3-TM5dbrynU-zGC4mj0B5KMIC"  # 32.1.15
  dest_path: "android_31_google_atd_x64"
}

system_image_package {
  package_name: "chromium/third_party/android_sdk/public/system-images/android-31/google_atd/x86_64"
  version: "4yT_R1tLQ3TP4_DMci8-FhYcNSFP3eaVSeSus3vWDmoC"  # r2
  dest_path: "android_31_google_atd_x64"
}
system_image_name: "system-images;android-31;google_atd;x86_64"

avd_package {
  package_name: "chromium/third_party/android_sdk/public/avds/android-31/google_atd/x86_64"
  dest_path: "android_31_google_atd_x64"
}
avd_name: "android_31_google_atd_x64"

avd_settings {
  screen {
    density: 480
    height: 1920
    width: 1080
  }
  advanced_features {
    key: "GLESDynamicVersion"
    value: "on"
  }
}
