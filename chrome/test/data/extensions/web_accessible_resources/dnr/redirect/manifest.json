{
  "name": "Test",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["war.js"],
      "matches": ["<all_urls>"],
      "use_dynamic_url": true
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "path": "rules_1.json",
        "enabled": true
      }
    ]
  }
}
