module gfx.mojom;

import "ui/gfx/geometry/mojom/geometry.mojom";

union TransformData {
  gfx.mojom.AxisTransform2d axis_2d;
  // Column major order.
  array<double, 16> matrix;
};

// See ui/gfx/transform.h.
struct Transform {
  // If data is null, this is an identity transform.
  TransformData? data;
};