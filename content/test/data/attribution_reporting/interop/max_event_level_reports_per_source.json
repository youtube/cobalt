{
  "description": "1 navigation source and 1 trigger produce 1 report",
  "input": {
    "registrations": [
      {
        // Should register but should not result in any reports.
        "timestamp": "0",
        "registration_request": {
          "context_origin": "https://source.test",
          "attribution_src_url": "https://reporter.test/register-source",
          "source_type": "navigation"
        },
        "responses": [
          {
            "url": "https://reporter.test/register-source",
            "debug_permission": true,
            "response": {
              "Attribution-Reporting-Register-Source": {
                "destination": "https://destination.test",
                "max_event_level_reports": 0,
                "source_event_id": "123"
              }
            }
          }
        ]
      },
      {
        // Should match but not result in an event-level report.
        "timestamp": "1000",
        "registration_request": {
          "attribution_src_url": "https://reporter.test/register-trigger",
          "context_origin": "https://destination.test"
        },
        "responses": [
          {
            "url": "https://reporter.test/register-trigger",
            "debug_permission": true,
            "response": {
              "Attribution-Reporting-Register-Trigger": {
                "debug_reporting": true,
                "event_trigger_data": [
                  {
                    "trigger_data": "2"
                  }
                ]
              }
            }
          }
        ]
      },
      {
        // Should register and only result in one report.
        "timestamp": "2000",
        "registration_request": {
          "context_origin": "https://source.test",
          "attribution_src_url": "https://reporter2.test/register-source",
          "source_type": "navigation"
        },
        "responses": [
          {
            "url": "https://reporter2.test/register-source",
            "debug_permission": true,
            "response": {
              "Attribution-Reporting-Register-Source": {
                "destination": "https://destination2.test",
                "max_event_level_reports": 1,
                "source_event_id": "456"
              }
            }
          }
        ]
      },
      {
        // Should result in an event-level report.
        "timestamp": "3000",
        "registration_request": {
          "attribution_src_url": "https://reporter2.test/register-trigger",
          "context_origin": "https://destination2.test"
        },
        "responses": [
          {
            "url": "https://reporter2.test/register-trigger",
            "debug_permission": true,
            "response": {
              "Attribution-Reporting-Register-Trigger": {
                "debug_reporting": true,
                "event_trigger_data": [
                  {
                    "trigger_data": "3"
                  }
                ]
              }
            }
          }
        ]
      },
      {
        // Should not result in an event-level report.
        "timestamp": "4000",
        "registration_request": {
          "attribution_src_url": "https://reporter2.test/register-trigger",
          "context_origin": "https://destination2.test"
        },
        "responses": [
          {
            "url": "https://reporter2.test/register-trigger",
            "debug_permission": true,
            "response": {
              "Attribution-Reporting-Register-Trigger": {
                "debug_reporting": true,
                "event_trigger_data": [
                  {
                    "trigger_data": "4"
                  }
                ]
              }
            }
          }
        ]
      }
    ]
  },
  "output": {
    "unparsable_registrations": [],
    "reports": [
      {
        "payload": [
          {
            "body": {
              "attribution_destination": "https://destination.test",
