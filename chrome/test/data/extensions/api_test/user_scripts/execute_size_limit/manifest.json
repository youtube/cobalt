{
  "name": "User script test extension",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Tests user scripts are not loaded when they surpass the size limit.",
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "permissions": [
    "scripting",
    "userScripts",
    "webNavigation"
  ],
  "host_permissions": [
    "*://*.requested.com/*"
  ]
}
