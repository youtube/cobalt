{
  "name": "chrome.i18n",
  "version": "0.1",
  "manifest_version": 2,
  "description": "end-to-end browser test for chrome.i18n API",
  "background": {
    "scripts": ["test.js"],
    "persistent": true
  },
  "permissions": ["http://*/*", "tabs"],
  "default_locale": "en_US",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["a.js"]
    }
  ]
}
