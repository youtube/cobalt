<!DOCTYPE translationbundle>
<translationbundle lang="kn">
<translation id="1287053896835709737">ಕಪ್ಪು ಬಣ್ಣದ ಮೇಲೆ ಹಳದಿ</translation>
<translation id="1408730541890277710">ಚಿತ್ರಗಳನ್ನು ಅವುಗಳ ಪರ್ಯಾಯ ಪಠ್ಯದೊಂದಿಗೆ ಬದಲಾಯಿಸಿ.</translation>
<translation id="145360476452865422">ಆನಿಮೇಶನ್ ನೀತಿ:</translation>
<translation id="1555130319947370107">ನೀಲಿ</translation>
<translation id="1588438908519853928">ಸಾಮಾನ್ಯ</translation>
<translation id="1591070050619849194">ಎಲ್ಲಾ ಚಿತ್ರ ಆನಿಮೇಶನ್ ಅನ್ನು ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಿ.</translation>
<translation id="1703735871906654364">ಕೆರೆಟ್ ಬ್ರೌಸಿಂಗ್</translation>
<translation id="1791496371305830581">ಅನಿಮೇಟ್ ಮಾಡಿರುವ ಎಲ್ಲಾ ಚಿತ್ರಗಳನ್ನು ಅನುಮತಿಸಿ.</translation>
<translation id="1974060860693918893">ಸುಧಾರಿತ</translation>
<translation id="1996252509865389616">ಸಕ್ರಿಯಗೊಳಿಸುವುದೇ?</translation>
<translation id="2079545284768500474">ರದ್ದುಮಾಡಿ</translation>
<translation id="2179565792157161713">ದೀರ್ಘ ವಿವರಣೆಯನ್ನು ಹೊಸ ಟ್ಯಾಬ್‌ನಲ್ಲಿ ತೆರೆಯಿರಿ</translation>
<translation id="2223143012868735942">ಬಣ್ಣದ ಗ್ರಹಿಕೆಯನ್ನು ಸುಧಾರಿಸಲು ವೆಬ್‌ಪುಟಗಳಿಗೆ ಕಸ್ಟಮೈಸ್ ಮಾಡಬಹುದಾದ ಬಣ್ಣ ಫಿಲ್ಟರ್ ಅನ್ನು ಅನ್ವಯಿಸಲಾಗಿದೆ.</translation>
<translation id="2394933097471027016">ಈಗಲೇ ಪ್ರಯತ್ನಿಸಿ - ಈ ಪುಟದಲ್ಲಿ ಕೆರೆಟ್ ಬ್ರೌಸಿಂಗ್ ಅನ್ನು ಯಾವಾಗಲೂ ಸಕ್ರಿಯಗೊಳಿಸಲಾಗಿರುತ್ತದೆ!</translation>
<translation id="2471847333270902538"><ph name="SITE" /> ಗೆ ಬಣ್ಣದ ಯೋಜನೆ:</translation>
<translation id="2648340354586434750">ಪದಗಳ ಮೂಲಕ ಸರಿಸಲು &lt;span class='key'&gt;ಆಯ್ಕೆ&lt;/span&gt; ಅನ್ನು ಹಿಡಿದುಕೊಳ್ಳಿ.</translation>
<translation id="2795227192542594043">ಈ ವಿಸ್ತರಣೆಯು ವೆಬ್ ಪುಟದಲ್ಲಿ ಸರಿಸಬಹುದಾದ ಕರ್ಸರ್ ಅನ್ನು ನಿಮಗೆ ನೀಡುತ್ತದೆ, ಕೀಬೋರ್ಡ್ ಮೂಲಕ ಪಠ್ಯವನ್ನು ಆಯ್ಕೆಮಾಡಲು ನಿಮಗೆ ಅನುಮತಿಸುತ್ತದೆ.</translation>
<translation id="2808027189040546825">ಹಂತ 1: ಮುಸುಕಾದ ನಕ್ಷತ್ರಗಳೊಂದಿಗೆ ಸಾಲನ್ನು ಆಯ್ಕೆಮಾಡಿ:</translation>
<translation id="2965611304828530558">&lt;p&gt;ನೀವು ಲಿಂಕ್ ಅಥವಾ ನಿಯಂತ್ರಣವನ್ನು ತಲುಪಿದಾಗ, ಅದನ್ನು ಸ್ವಯಂಚಾಲಿತವಾಗಿ ಕೇಂದ್ರೀಕರಿಸಲಾಗುತ್ತದೆ. ಲಿಂಕ್ ಅಥವಾ ಬಟನ್ ಕ್ಲಿಕ್ ಮಾಡಲು &lt;span class='key'&gt;Enter&lt;/span&gt; ಅನ್ನು ಒತ್ತಿ. &lt;/p&gt; &lt;p&gt;  ಕೆರೆಟ್ ಬ್ರೌಸಿಂಗ್ ಅನ್ನು ಮುಂದುವರಿಸಲು, ಕೇಂದ್ರೀಕರಿಸಲಾದ ನಿಯಂತ್ರಣವು (ಪಠ್ಯ ಬಾಕ್ಸ್ ಅಥವಾ ಪಟ್ಟಿ ಬಾಕ್ಸ್‌ನಂತಹ) ಬಾಣದ ಕೀಗಳನ್ನು ಸೆರೆಹಿಡಿಯುತ್ತಿದ್ದರೆ, &lt;span class='key'&gt;Esc&lt;/span&gt;  ಒತ್ತಿರಿ ನಂತರ ಎಡ ಅಥವಾ ಬಲ ಬಾಣವನ್ನು ಒತ್ತಿರಿ. &lt;/p&gt; &lt;p&gt;   ಪರ್ಯಾಯವಾಗಿ, ಮುಂದಿನ ಕೇಂದ್ರೀಕರಿಸಬಹುದಾದ ನಿಯಂತ್ರಣಕ್ಕೆ ಸರಿಸಲು &lt;span class='key'&gt;Esc&lt;/span&gt; ಅನ್ನು ಒತ್ತಿ. &lt;/p&gt;</translation>
<translation id="3252573918265662711">ಸೆಟಪ್</translation>
<translation id="3410969471888629217">ಸೈಟ್ ಕಸ್ಟಮೈಸ್ ಮಾಡುವಿಕೆಯನ್ನು ಮರೆತುಬಿಡಿ</translation>
<translation id="3435896845095436175">ಸಕ್ರಿಯಗೊಳಿಸು</translation>
<translation id="3622586652998721735">ಡಿಫಾಲ್ಟ್ ಯೋಜನೆಯಂತೆ ಹೊಂದಿಸಿ</translation>
<translation id="3812541808639806898">ಚಿತ್ರ Alt ಪಠ್ಯ ವೀಕ್ಷಕ</translation>
<translation id="381767806621926835">ಅದರ ದೀರ್ಘ ವಿವರಣೆಯನ್ನು ಪ್ರವೇಶಿಸಲು "longdesc" ಅಥವಾ "aria-describedat" ಗುಣಲಕ್ಷಣದೊಂದಿಗೆ ಯಾವುದರ ಮೇಲಾದರೂ ರೈಟ್-ಕ್ಲಿಕ್ ಮಾಡಿ.</translation>
<translation id="4023902424053835668">ಬಾಣದ ಕೀಗಳನ್ನು ಬಳಸುವ ಮೂಲಕ ವೆಬ್ ಪುಟಗಳ ಪಠ್ಯವನ್ನು ಬ್ರೌಸ್ ಮಾಡಿ.</translation>
<translation id="4388820049312272371">ತ್ವರಿತ ಫ್ಲಾಶ್ ಜೊತೆಗೆ ಕರ್ಸರ್ ಸ್ಥಾನವನ್ನು ಹೈಲೈಟ್ ಮಾಡಿ.</translation>
<translation id="4394049700291259645">ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಿ</translation>
<translation id="4769065380738716500">ಚಿತ್ರಗಳನ್ನು ಅದರ ಪರ್ಯಾಯ ಪಠ್ಯದೊಂದಿಗೆ ಮರುಸ್ಥಾನಗೊಳಿಸಲಾಗಿದೆ.</translation>
<translation id="4896660567607030658">ಪ್ರತಿಕ್ರಿಯೆ ನೀಡದೆ, ಕೇವಲ ಕರ್ಸರ್ ಅನ್ನು ತೋರಿಸಿ.</translation>
<translation id="4937901943818762779">ಆನಿಮೇಟ್ ಮಾಡಿರುವ ಚಿತ್ರಗಳನ್ನು ಅನುಮತಿಸಿ, ಆದರೆ ಒಮ್ಮೆ ಮಾತ್ರ.</translation>
<translation id="4949131196216960195">ಕೆರೆಟ್ ಬ್ರೌಸಿಂಗ್ ಅನ್ನು ಆನ್ ಮಾಡಲು &lt;span class='key'&gt;ಹುಡುಕಾಟ&lt;/span&gt; + &lt;img src='increase_brightness.png'&gt; (ಪ್ರಖರತೆಯನ್ನು ಅಧಿಕಗೊಳಿಸುವ ಕೀ ಅಥವಾ F7) ಅನ್ನು ಒತ್ತಿ. ಆಫ್ ಮಾಡಲು ಅದನ್ನು ಪುನಃ ಒತ್ತಿ.</translation>
<translation id="4954450790315188152">ಕೆರೆಟ್ ಬ್ರೌಸಿಂಗ್ ಸಕ್ರಿಯಗೊಳಿಸಲಾಗಿದೆ:</translation>
<translation id="5041932793799765940">ಬಣ್ಣದ ಹೊಂದಾಣಿಕೆ</translation>
<translation id="5094574508723441140">ಹೆಚ್ಚಿಸಿದ ವೈದೃಶ್ಯ</translation>
<translation id="5173942593318174089">ಅನಿಮೇಷನ್ ಜೊತೆಗೆ ಕರ್ಸರ್ ಸ್ಥಾನವನ್ನು ಹೈಲೈಟ್ ಮಾಡಿ.</translation>
<translation id="5287723860611749454">&lt;p&gt;ಡಾಕ್ಯುಮೆಂಟ್‌ನಾದ್ಯಂತ ಸರಿಸಲು ಬಾಣದ ಗುರುತುಗಳನ್ನು ಬಳಸಿ. &lt;/p&gt;&lt;p&gt;ಕರ್ಸರ್ ಅನ್ನು ಆ ಸ್ಥಳಕ್ಕೆ ಸರಿಸಲು ಎಲ್ಲಾದರೂ ಕ್ಲಿಕ್ ಮಾಡಿ. &lt;/p&gt; &lt;p&gt;   ಪಠ್ಯವನ್ನು ಆಯ್ಕೆಮಾಡಲು &lt;span class='key'&gt;Shift&lt;/span&gt; + ಬಾಣಗಳನ್ನು ಒತ್ತಿರಿ.&lt;/p&gt;</translation>
<translation id="5331422999063554397">ತಿರುಗುಮುರುಗಾದ ಬಣ್ಣ</translation>
<translation id="5555153510860501336">ಅಧಿಕ ವೈದೃಶ್ಯವನ್ನು ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಲಾಗಿದೆ</translation>
<translation id="5558600050691192317">ಕೀಬೋರ್ಡ್ ಆದೇಶಗಳು</translation>
<translation id="5594989420907487559">ಅನಿಮೇಶನ್‌‌ಗಳನ್ನು ಒಮ್ಮೆ ಮಾತ್ರ ರನ್ ಮಾಡಿ ಅಥವಾ ಸಂಪೂರ್ಣವಾಗಿ ಆನಿಮೇಶನ್ ಅನ್ನು ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಿ.</translation>
<translation id="5631241868147802353">ಡೀಫಾಲ್ಟ್ ಬಣ್ಣ ಯೋಜನೆ:</translation>
<translation id="5650358096585648000">ದೃಶ್ಯಾವಳಿ ಪ್ರತಿಕ್ರಿಯೆ</translation>
<translation id="5710185147685935461">ವೆಬ್‌ಪುಟಗಳನ್ನು ಓದಲು ಸುಲಭಗೊಳಿಸಲು ಬಣ್ಣದ ಯೋಜನೆಯನ್ನು ಬದಲಾಯಿಸಿ ಅಥವಾ ತಿರುಗಿಸಿ.</translation>
<translation id="5939518447894949180">ಮರುಹೊಂದಿಸು</translation>
<translation id="595639123821853262">ತಿರುಗುಮುರುಗಾದ ಗ್ರೇಸ್ಕೇಲ್</translation>
<translation id="6017514345406065928">ಹಸಿರು</translation>
<translation id="6050189528197190982">ಗ್ರೇಸ್ಕೇಲ್</translation>
<translation id="6170146920149900756">ಬಣ್ಣ ವರ್ಧಕ</translation>
<translation id="633394792577263429">ಪದಗಳ ಮೂಲಕ ಸರಿಸಲು &lt;span class='key'&gt;Control&lt;/span&gt; ಅನ್ನು ಹಿಡಿದುಕೊಳ್ಳಿ.</translation>
<translation id="6550675742724504774">ಆಯ್ಕೆಗಳು</translation>
<translation id="6699630355767768222">ಹಂತ 3: ತಿದ್ದುಪಡಿಗಾಗಿ ಬಣ್ಣದ ಅಕ್ಷವನ್ನು ಓವರ್‌ರೈಡ್ ಮಾಡಿ.</translation>
<translation id="6838518108677880446">ಸೆಟಪ್:</translation>
<translation id="690628312087070417">ದೀರ್ಘವಾದ ಅಂತರದಿಂದ ಕೆರೆಟ್ ಮುಂದಕ್ಕೆ ಸಾಗಿದಾಗ:</translation>
<translation id="6965382102122355670">ಸರಿ</translation>
<translation id="7379645913608427028">ಡಿಗ್ರಿ</translation>
<translation id="7384431257964758081">ಅಧಿಕ ವೈದೃಶ್ಯವನ್ನು ಸಕ್ರಿಯಗೊಳಿಸಲಾಗಿದೆ</translation>
<translation id="7586636300921797327">ಹಂತ 2: ಆಯ್ಕೆಮಾಡಿದ ಸಾಲಿನಲ್ಲಿ ಎಲ್ಲಾ ನಕ್ಷತ್ರಗಳು ಗೋಚರಿಸುವವರೆಗೆ ಸ್ಲೈಡರ್
        ಅನ್ನು ಹೊಂದಿಸಿ</translation>
<translation id="7658239707568436148">ರದ್ದುಮಾಡಿ</translation>
<translation id="786423340267544509">aria-describedat ಅಥವಾ longdesc ಗುಣಲಕ್ಷಣಗಳ ಜೊತೆಗೆ ಅಂಶಗಳಿಗೆ ಅಂಚು ಸೇರಿಸಿ.</translation>
<translation id="7942349550061667556">ಕೆಂಪು</translation>
<translation id="8260673944985561857">ಕೆರೆಟ್ ಬ್ರೌಸಿಂಗ್ ಆಯ್ಕೆಗಳು</translation>
<translation id="8321034316479930120">ಆನಿಮೇಶನ್ ನೀತಿ</translation>
<translation id="8480209185614411573">ಅಧಿಕ ವೈದೃಶ್ಯ</translation>
<translation id="8609925175482059018">ಕೆರೆಟ್ ಬ್ರೌಸಿಂಗ್ ಆನ್ ಮಾಡಲು &lt;span class='key'&gt;F7&lt;/span&gt; ಅನ್ನು ಒತ್ತಿರಿ. ಆಫ್ ಮಾಡಲು ಅದನ್ನು ಪುನಃ ಒತ್ತಿರಿ.</translation>
<translation id="8798099450830957504">ಡಿಫಾಲ್ಟ್</translation>
<translation id="894241283505723656">ಸಂದರ್ಭ ಮೆನುವಿನಲ್ಲಿ ದೀರ್ಘ ವಿವರಣೆಗಳು</translation>
</translationbundle>