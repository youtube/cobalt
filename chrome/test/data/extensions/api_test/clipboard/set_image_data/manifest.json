{
  "manifest_version": 2,
  "name": "chrome.clipboard.set_image_data",
  "minimum_chrome_version": "56.0.0.0",
  "version": "1.0",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "clipboard",
    "clipboardRead",
    "clipboardWrite",
    "http://localhost/*"
  ]
}
