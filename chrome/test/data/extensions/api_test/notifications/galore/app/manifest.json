{
  "name": "Notifications Galore!",
  "description": "A testbed for Chrome notifications",
  "version": "0.9.6",
  "manifest_version": 2,
  "minimum_chrome_version": "27",
  "app": {
    "background": {
      "scripts": ["view.js", "controller.js", "main.js"]
    }
  },
  "icons": {
    "16": "images/icon-16x16.png",
    "48": "images/icon-48x48.png",
    "128": "images/icon-128x128.png"
  },
  "permissions": [
    "notifications",
    "storage",
    "https://calendar.google.com/googlecalendar/images/about-alerts.gif",
    "https://www.google.com/images/icons/product/googlemail-64.png"
  ]
}
