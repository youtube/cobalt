{
  "docker_service": "linux",
  "platforms": [
    "chromium_android-arm"
  ],
  "gn_args": [
    {
      "disable_xr": true,
      "disable_bluetooth" : true,
      "target_os": "android",
      "target_cpu": "arm"
    }
  ],
  "targets": [
    "content_shell",
    "system_webview_apk",
    "system_webview_shell_apk"
  ],
  "includes": [
    {
      "name": "arm",
      "platform": "chromium_android-arm"
    }
  ]
}
