source_set("icon_standardizer") {
  sources = [
    "icon_standardizer.cc",
    "icon_standardizer.h",
  ]

  deps = [
    "//base",
    "//skia",
    "//ui/gfx",
  ]
}

source_set("user_type_filter") {
  sources = [
    "user_type_filter.cc",
    "user_type_filter.h",
  ]

  deps = [
    "//base",
    "//build:chromeos_buildflags",
    "//chrome/browser/profiles:profile",
    "//content/public/browser:browser",
  ]

  if (is_chromeos) {
    deps += [ "//chromeos/components/mgs" ]
  }
}
