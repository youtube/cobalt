module mojo_base.mojom;

[Stable]
struct Time {
  // The internal value is expressed in terms of microseconds since a fixed but
  // intentionally unspecified epoch.
  int64 internal_value;
};

[Stable]
struct TimeDelta {
  int64 microseconds;
};

[Stable]
struct TimeTicks {
  // The internal value is expressed in terms of microseconds since a fixed but
  // intentionally unspecified epoch.
  int64 internal_value;
};
