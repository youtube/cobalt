      tmp_BASENAME=`basename "$LLVM_SYMBOLIZER"`
      tmp_PWD=`cd "$tmp_DIRNAME" && pwd -W`
      LLVM_SYMBOLIZER="$tmp_PWD/$tmp_BASENAME"
      if test -e "$LLVM_SYMBOLIZER.exe"; then
        LLVM_SYMBOLIZER="$LLVM_SYMBOLIZER.exe"
