{
  "name": "User script test extension",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Tests a variety of user scripts API calls for registering an user script.",
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "permissions": [
    "userScripts",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "*://*.requested.com/*",
    "*://*.requested-2.com/*"
  ]
}
