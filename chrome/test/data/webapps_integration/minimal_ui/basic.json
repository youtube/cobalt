{
  "name": "Site B",
  "icons": [
    {
      "src": "basic-48.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "basic-192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "start_url": "/webapps_integration/minimal_ui/basic.html",
  "scope": "/webapps_integration/minimal_ui/",
  "display": "minimal-ui",
  "file_handlers": [
    {
      "action": "/webapps_integration/minimal_ui/foo_handler.html",
      "name": "Plain Text",
      "accept": {
        "application/octet-stream": [".foo"]
      }
    },
    {
      "action": "/webapps_integration/minimal_ui/bar_handler.html",
      "accept": {
        "application/octet-stream": [".bar"]
      },
      "launch_type": "multiple-clients"
    }
  ]
}
