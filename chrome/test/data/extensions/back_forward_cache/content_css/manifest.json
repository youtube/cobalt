{
  "name": "no caching",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Checks that injected CSS do prevent back forward cache.",
  "permissions": ["http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["color.css"],
      "run_at": "document_start"
    }
  ]
}
