module ash.common.mojom;

// Interface for sending logs to the browser that will be logged as SYSLOGs.
interface WebUiSyslogEmitter {
  // Emits a 'SYSLOG()' with the format "{prefix} : {message}".
  EmitSyslog(string prefix, string message);
};
