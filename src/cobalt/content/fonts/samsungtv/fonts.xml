        <font font_name="Samsung One UI 600" postscript_name="SamsungOneUI-600" style="normal" weight="600">SamsungOneUI-600.ttf</font>
    </family>
    <alias name="times" to="serif" />
    <alias name="times new roman" to="serif" />
