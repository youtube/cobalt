      "id":"config.validate_config",
      "path":"validate-config",
      "request":{
        "$ref":"LuciConfigValidateConfigRequestMessage",
        "parameterName":"resource"
      },
      "response":{
        "$ref":"LuciConfigValidateConfigResponseMessage"
      },
      "scopes":[
        "https://www.googleapis.com/auth/userinfo.email"
      ]
    }
