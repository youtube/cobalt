{
  "name": "Test extension",
  "declarative_net_request": {
    "rule_resources": [{
      "id": "rules",
      "path": "rules.json",
      "enabled": true
    }]
  },
  "manifest_version": 2,
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "<all_urls>",
    "tabs"
  ],
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
