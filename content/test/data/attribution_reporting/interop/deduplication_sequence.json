{
  "description": "Deduplication is maintained even as event-level reports may be replaced due to a new trigger's priority.",
  "input": {
    "registrations": [
      {
        "registration_request": {
          "context_origin": "https://source.test",
          "Attribution-Reporting-Eligible": "event-source"
        },
        "responses": [
          {
            "url": "https://reporter.test/register-source",
            "response": {
              "Attribution-Reporting-Register-Source": {
                "source_event_id": "123",
                "destination": "https://destination.test",
                "expiry": "86400"
              }
            }
          }
        ],
        "timestamp": "0"
      },
      // Trigger generates an event report.
      {
        "registration_request": {
          "context_origin": "https://destination.test"
        },
        "responses": [
          {
            "url": "https://reporter.test/register-trigger",
            "response": {
              "Attribution-Reporting-Register-Trigger": {
                "event_trigger_data": [
                  {
                    "trigger_data": "2",
                    "priority": "0",
                    "deduplication_key": "1"
                  }
                ]
              }
            }
          }
        ],
        "timestamp": "1000"
      },
      // Attribution replaces the event report generated by the previous trigger
      // since event-trigger priority is greater.
      {
        "registration_request": {
          "context_origin": "https://destination.test"
        },
        "responses": [
          {
            "url": "https://reporter.test/register-trigger",
            "response": {
              "Attribution-Reporting-Register-Trigger": {
                "event_trigger_data": [
                  {
                    "trigger_data": "3",
                    "priority": "1",
                    "deduplication_key": "2"
                  }
                ]
              }
            }
          }
        ],
        "timestamp": "2000"
      },
      // Event-trigger is deduplicated by the first trigger, although its
      // associated report was already replaced.
      {
        "registration_request": {
          "context_origin": "https://destination.test"
        },
        "responses": [
          {
            "url": "https://reporter.test/register-trigger",
            "response": {
              "Attribution-Reporting-Register-Trigger": {
                "event_trigger_data": [
                  {
                    "trigger_data": "4",
                    "priority": "2",
                    "deduplication_key": "1"
                  }
                ]
              }
            }
          }
        ],
        "timestamp": "3000"
      }
    ]
  },
  "output": {
    "reports": [
      {
        "report_time": "86400000",
        "payload": {
          "attribution_destination": "https://destination.test",
          "scheduled_report_time": "86400",
          "source_event_id": "123",
          "trigger_data": "1",
          "source_type": "event",
          "randomized_trigger_rate": 0.0000025
        },
        "report_url": "https://reporter.test/.well-known/attribution-reporting/report-event-attribution"
      }
    ]
  }
}
