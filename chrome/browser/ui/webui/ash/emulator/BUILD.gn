import("//build/config/chromeos/ui_mode.gni")
import("//chromeos/dbus/config/use_real_dbus_clients.gni")

assert(is_chromeos_ash)
assert(!is_official_build)
assert(!use_real_dbus_clients)

static_library("emulator") {
