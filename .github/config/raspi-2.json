{
  "docker_service": "build-raspi",
  "on_device_test": {
    "enabled": true,
    "tests": [
      "0",
      "1",
      "2",
      "3",
      "4",
      "5"
    ]
  },
  "platforms": [
    "raspi-2",
    "raspi-2-sbversion-17"
  ],
  "includes": [
    {
      "name":"raspi",
      "platform":"raspi-2",
      "target_platform":"raspi-2",
      "target_cpu":"target_cpu=\\\"arm\\\"",
      "extra_gn_arguments": "build_with_separate_cobalt_toolchain=true use_asan=false"
    },
    {
      "name":"sbversion-17",
      "platform":"raspi-2-sbversion-17",
      "target_platform":"raspi-2",
      "target_cpu":"target_cpu=\\\"arm\\\"",
      "extra_gn_arguments": "build_with_separate_cobalt_toolchain=true use_asan=false",
      "sb_api_version": "17"
    }
  ]
}
