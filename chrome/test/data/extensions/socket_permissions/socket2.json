{
  "name": "effective host permisions test",
  "version": "1.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {"socket": [
      "tcp-connect",
      "tcp-connect:example.com",
      "tcp-connect:example.com:80",
      "tcp-connect:example.com:*",
      "tcp-connect:*.example.com",
      "tcp-listen",
      "tcp-listen::8080",
      "udp-bind::8888",
      "udp-send-to:239.255.255.250:1900"
    ]}
  ]
}
