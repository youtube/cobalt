// https://www.w3.org/TR/webnn/

enum MLOperandType {
  "float32",
  "float16",
  "int32",
  "uint32",
  "int8",
  "uint8"
};

dictionary MLOperandDescriptor {
  required MLOperandType type;
  sequence<[EnforceRange] unsigned long> dimensions;
};
