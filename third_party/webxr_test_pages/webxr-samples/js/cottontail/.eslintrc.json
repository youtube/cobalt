{
  "extends": ["eslint:recommended","google"],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "XRWebGLLayer": true
  },
  "rules": {
    "max-len": [2,120],
    "valid-jsdoc": [0, {}],
    "require-jsdoc": [0, {}],
    "no-console": 0,
    "guard-for-in": 0
  }
}
