        "report_url": "https://another-reporter.test/.well-known/attribution-reporting/debug/verbose"
      },
      {
        "payload": {
          "attribution_destination": [
            "https://another-destination.test",
            "https://destination.test"
          ],
          "randomized_trigger_rate": 0.0024263,
