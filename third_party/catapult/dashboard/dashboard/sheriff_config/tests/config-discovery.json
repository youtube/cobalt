        "$ref":"LuciConfigGetConfigByHashResponseMessage"
      },
      "scopes":[
        "https://www.googleapis.com/auth/userinfo.email"
      ]
    },
    "get_config_sets":{
      "description":"Returns config sets.",
      "httpMethod":"GET",
      "id":"config.get_config_sets",
      "parameters":{
        "config_set":{
          "location":"query",
          "type":"string"
        },
        "include_files":{
          "location":"query",
          "type":"boolean"
        },
        "include_last_import_attempt":{
          "location":"query",
          "type":"boolean"
        }
