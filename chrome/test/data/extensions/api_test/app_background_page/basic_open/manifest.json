{
  "name": "app_background_page/basic_open",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Tests that web-extent background pages can be opened.",
  "background": {
    "page": "test.html"
  },
  "permissions": ["tabs", "http://a.com/*"],
  "content_scripts": [
    {
      "matches": ["http://a.com/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}
