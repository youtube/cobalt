{
  "name": "isolated world 1",
  "version": "0.1",
  "manifest_version": 2,
  "description": "tests that all scripts from an extension run in the same isolated world",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": ["http://*/*", "tabs"],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["a.js"]
    },
    {
      "matches": ["http://*/*"],
      "js": ["b.js"]
    }
  ]
}
