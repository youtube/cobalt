{
  "name": "Manifest with tab strip customizations",
  "icons": [
    {
      "src": "basic-192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "start_url": "/web_apps/get_manifest.html?tab_strip_wildcard_home_scope.json",
  "scope": "/",
  "display": "standalone",
  "display_override": [ "tabbed" ],
  "tab_strip": {
    "home_tab": {
      "scope_patterns": [
        {"pathname": "/web_apps/*"}
      ]
    }
  }
}
