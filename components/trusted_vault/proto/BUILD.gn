import("//third_party/protobuf/proto_library.gni")

proto_library("proto") {
  sources = [
    "local_trusted_vault.proto",
    "vault.proto",
  ]

  extra_configs = [ "//build/config/compiler:wexit_time_destructors" ]
}

if (is_android) {
  import("//build/config/android/rules.gni")

  proto_java_library("proto_java") {
    proto_path = "//"
    sources = [ "vault.proto" ]
  }
}
