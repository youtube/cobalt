{
  "name": "Combined message permissions",
  "version": "1.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["doesnt_exist.js"]
    }
  },
  "permissions": ["http://api.flickr.com/",
                  "bookmarks",
                  "geolocation",
                  "history",
                  "management",
                  "tabs",
                  "audioCapture",
                  "videoCapture"]
}
