import os
import sys

if __name__ == '__main__':
  tracing_path = os.path.abspath(os.path.join(os.path.dirname(__file__),
                                              '..'))
  sys.path.append(tracing_path)
  from tracing.mre import map_traces
  sys.exit(map_traces.Main(sys.argv))
