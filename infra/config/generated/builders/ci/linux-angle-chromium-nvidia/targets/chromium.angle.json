      },
      {
        "args": [
          "webgl2_conformance",
          "--show-stdout",
          "--browser=release",
          "--passthrough",
          "-v",
          "--stable-jobs",
          "--extra-browser-args=--enable-logging=stderr --js-flags=--expose-gc --use-gl=angle --use-angle=gl --use-cmd-decoder=passthrough --force_high_performance_gpu",
