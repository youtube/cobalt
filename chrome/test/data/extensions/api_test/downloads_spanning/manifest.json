{
  "name": "downloads incognito spanning apitest",
  "version": "0.1",
  "manifest_version": 3,
  "description": "downloads incognito spanning apitest",
  "incognito": "spanning",
  "web_accessible_resources": [
    {
      "resources": ["empty.html"],
      "matches": ["*://*/*"]
    }
  ],
  "permissions": [
    "downloads",
    "downloads.ui"
  ],
  "host_permissions": [
    "http://*/*",
    "file://*"
  ]
}
