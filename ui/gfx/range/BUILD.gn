component("range") {
  sources = [
    "gfx_range_export.h",
    "range.cc",
    "range.h",
    "range_f.cc",
    "range_f.h",
  ]

  if (is_apple) {
    sources += [ "range_mac.mm" ]
  }

  defines = [ "GFX_RANGE_IMPLEMENTATION" ]

  deps = [
    "//base",
    "//ui/gfx:gfx_export",
  ]
}
