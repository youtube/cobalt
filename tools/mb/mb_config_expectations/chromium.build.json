{
  "android-build-perf-developer": {
    "builtin": {
      "gn_args": {
        "enable_nacl": false,
        "enable_precompiled_headers": false,
        "is_component_build": true,
        "is_debug": true,
        "symbol_level": 2,
        "target_cpu": "arm64",
        "target_os": "android"
      }
    },
    "reproxy": {
      "gn_args": {
        "enable_nacl": false,
