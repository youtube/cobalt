  root_dir = "."
  out_dir = "$target_gen_dir/tsc"
  composite = true
  tsconfig_base = "tsconfig_base.json"
  in_files = [
    "chai_assert.ts",
    "fake_chrome_event.ts",
    "keyboard_mock_interactions.ts",
    "mocha_adapter_simple.ts",
    "mock_controller.ts",
    "mock_timer.ts",
    "mock_timer_test.ts",
