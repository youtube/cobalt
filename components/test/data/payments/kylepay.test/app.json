{
  "name": "Pay with KylePay",
  "short_name": "KylePay",
  "icons": [{
    "src": "icon.png",
    "sizes": "48x48",
    "type": "image/png"
  }],
  "serviceworker": {
    "src": "app.js",
    "use_cache": false,
    "scope": "/webpay"
  },
  "payment": {
    "supported_delegations": ["shippingAddress", "payerName", "payerEmail", "payerPhone"]
  }
}
