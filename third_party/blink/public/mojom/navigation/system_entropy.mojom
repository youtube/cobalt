module blink.mojom;

enum SystemEntropy {
  // Entropy state will be empty for all non-top-level navigation, and popups.
  kEmpty,

  // Indicates that navigation occurred while the user agent was in a normal
  // entropy state.
  kNormal,

  // Indicates that navigation occurred while the user agent was in a high
  // entropy state.
  kHigh,

};
