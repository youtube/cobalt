{
  "name": "Manifest with tab strip customizations",
  "icons": [
    {
      "src": "basic-192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "start_url": "get_manifest.html",
  "display": "standalone",
  "display_override": [ "tabbed" ],
  "tab_strip": {
    "home_tab": {
      "scope_patterns": [
        {"pathname": "/web_apps/tab_strip_customizations.html"},
        {"pathname": "/web_apps/title_appname_prefix.html"}
      ]
    }
  }
}
