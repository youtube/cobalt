{
  "name": "downloads incognito split apitest",
  "version": "0.1",
  "manifest_version": 3,
  "description": "downloads incognito split apitest",
  "incognito": "split",
  "web_accessible_resources": [
    {
      "resources": ["empty.html"],
      "matches": ["*://*/*"]
    }
  ],
  "action": {
    "default_popup": "accept_danger.html"
  },
  "permissions": [
    "downloads",
    "downloads.open",
    "downloads.shelf",
    "downloads.ui"
  ],
  "host_permissions": [
    "http://*/*",
    "file://*"
  ]
}
