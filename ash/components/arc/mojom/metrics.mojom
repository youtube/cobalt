  kBinderReleaseClipboardData = 1,
  kWlBufferRelease = 2,
  kWlKeyboardLeave = 3,
  kWlPointerMotion = 4,
  kWlPointerLeave = 5,
  kZauraShellActivated = 6,
  kZauraSurfaceOcclusionChanged = 7,
  kZcrRemoteSurfaceWindowGeometryChanged = 8,
  kZcrRemoteSurfaceBoundsChangedInOutput = 9,
  kZcrVsyncTimingUpdate = 10,
};

