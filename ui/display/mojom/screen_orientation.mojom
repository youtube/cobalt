module display.mojom;

// The screen orientation. This is slightly different than the
// device.mojom.ScreenOrientationLockType which is a request to lock to
// a specific screen orientation.
enum ScreenOrientation {
  kUndefined,
  kPortraitPrimary,
  kPortraitSecondary,
  kLandscapePrimary,
  kLandscapeSecondary,
};
