{
  "name": "Host permissions manifest key",
  "version": "0.1",
  "manifest_version": 3,
  "description": "extension with hosts in |permissions|, |optional_permissions|, |host_permissions| and |optional_host_permissions|",
  "permissions": [
    "https://google.com/*"
  ],
  "optional_permissions": [
    "http://chromium.org/*"
  ],
  "host_permissions": [
    "https://example.com/*"
  ],
  "optional_host_permissions": [
    "https://optional.com/*"
  ]
}
