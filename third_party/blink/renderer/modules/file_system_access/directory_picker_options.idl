// https://wicg.github.io/file-system-access/#dictdef-directorypickeroptions
dictionary DirectoryPickerOptions {
  DOMString id;
  (WellKnownDirectory or FileSystemHandle) startIn;
  FileSystemPermissionMode mode = "read";
};
