{
  "linux-wpt-chromium-rel": {
    "isolated_scripts": [
      {
        "args": [
          "--inverted-test-launcher-filter-file=../../third_party/blink/web_tests/TestLists/chrome.filter",
          "--test-type",
          "testharness",
          "reftest",
          "crashtest",
          "print-reftest",
          "--exit-after-n-crashes-or-timeouts=10000",
          "--exit-after-n-failures=10000",
          "--no-retry-failures"
        ],
        "experiment_percentage": 100,
        "merge": {
          "args": [
            "--verbose"
          ],
          "script": "//third_party/blink/tools/merge_web_test_results.py"
        },
        "name": "headless_shell_wpt_tests_include_all",
        "resultdb": {
          "enable": true,
          "has_native_resultdb_integration": true
        },
        "results_handler": "layout tests",
        "swarming": {
          "dimensions": {
            "os": "Ubuntu-22.04"
          },
          "service_account": "chromium-tester@chops-service-accounts.iam.gserviceaccount.com",
          "shards": 15
        },
        "test": "headless_shell_wpt",
        "test_id_prefix": "ninja://:headless_shell_wpt/"
      }
    ]
  }
}