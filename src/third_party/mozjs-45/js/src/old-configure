# Debugging and Optimizations

if test "$ENABLE_METHODJIT_SPEW"; then
    cat >> confdefs.pytmp <<\EOF
    (''' JS_METHODJIT_SPEW ''', ' 1 ')
EOF
cat >> confdefs.h <<\EOF
