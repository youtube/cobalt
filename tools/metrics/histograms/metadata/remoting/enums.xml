<histogram-configuration>

<!-- Enum types -->

<enums>

<enum name="AudioHelperStartStreamResult">
  <int value="0" label="Success"/>
  <int value="1" label="Stream already started"/>
  <int value="2" label="Failed to create stream"/>
  <int value="3" label="Failed to open stream"/>
</enum>

<enum name="AudioPlaybackMode">
  <int value="0" label="Unknown"/>
  <int value="1" label="Local only"/>
  <int value="2" label="Remote and local"/>
  <int value="3" label="Remote only"/>
</enum>

<enum name="OpenOutcome">
  <int value="0" label="Success"/>
  <int value="1" label="Already open"/>
  <int value="2" label="Failed"/>
  <int value="3" label="Failed system permissions"/>
  <int value="4" label="Failed in use"/>
</enum>

</enums>

</histogram-configuration>
