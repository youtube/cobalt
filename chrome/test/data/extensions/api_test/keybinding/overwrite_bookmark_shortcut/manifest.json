{
  "name": "An extension that overwrites permitted system keybindings",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab"],
  "chrome_ui_overrides": {
    "bookmarks_ui": {
      "remove_bookmark_shortcut": true
    }
  },
  "commands": {
    "send message": {
      "suggested_key": "Ctrl+D",
      "description": "Send message"
    }
  }
}
