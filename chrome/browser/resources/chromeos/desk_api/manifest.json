{
  "manifest_version": 3,
  "name": "Desk API Component Extension",
  "externally_connectable": {
    "matches": [ "$DOMAIN_LIST" ],
    "accepts_tls_channel_id": false
  },
  "version": "0.3.1",
  "icons": {
    "128": "templates_icon.png"
  },
  "background": {
    "service_worker": "desk_api_bundle.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": [
    "notifications",
    "wmDesksPrivate"
  ],
  "key": ""
}
