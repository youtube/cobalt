{
  "name": "chrome.socket",
  "manifest_version": 2,
  "version": "0.1",
  "description": "end-to-end browser test for chrome.socket API",
  "app": {
    "background": {
      "scripts": ["multicast.js", "background.js"]
    }
  },
  "permissions": [
    {"socket": [
      "tcp-connect",
      "tcp-listen",
      "udp-send-to",
      "udp-bind",
      "udp-multicast-membership"
    ]}
  ]
}
