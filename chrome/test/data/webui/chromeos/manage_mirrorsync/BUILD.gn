import("../../build_webui_tests.gni")

build_webui_tests("build_webui_tests") {
  ts_path_mappings = [
    "chrome://manage-mirrorsync/*|" + rebase_path(
            "$root_gen_dir/chrome/browser/resources/chromeos/manage_mirrorsync/tsc/*",
            target_gen_dir),
    "chrome://webui-test/chromeos/*|" +
        rebase_path("$root_gen_dir/chrome/test/data/webui/chromeos/tsc/*",
                    target_gen_dir),
  ]

  files = [ "manage_mirrorsync_app_test.ts" ]
  ts_deps = [
    "..:build_ts",
    "//chrome/browser/resources/chromeos/manage_mirrorsync:build_ts",
    "//chrome/test/data/webui/chromeos:build_ts",
    "//ui/webui/resources/js:build_ts",
  ]
}
