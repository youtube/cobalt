{
  "name": "Platform App Test: <webview>",
  "version": "2",
  "manifest_version": 2,
  "permissions": [
    "webview"
  ],
  "app": {
    "background": {
      "scripts": ["test.js"]
    }
  },
  "webview": {
    "partitions": [
      {
        "name": "foobar",
        "accessible_resources": [
          "guest_with_inline_script.html"
        ]
      }
    ]
  }
}
